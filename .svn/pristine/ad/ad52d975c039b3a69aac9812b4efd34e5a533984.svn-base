body, div, p, ul, ol, li, nav, footer, dl, dt, dd, h1, h2, h3, h4, h5, h6, form, img { padding: 0; margin: 0; }
body { min-width: 320px; margin: 0 auto; font: normal 18px/1.5 helvetica, verdana, san-serif; background: #fafafa; overflow: hidden; height:100%;}
ul, ol, li { list-style: none; }
* {	-webkit-tap-highlight-color: rgba(255,255,255,0); outline:none;}
img { vertical-align: middle; display: block; border: none; }
a { text-decoration: none; }
.fl { float: left; }
.fr { float: right; }
.hide { display: none; }

/*颜色*/
.orange { color: #f50;}

/*布局*/
.main { width: 100%; max-width: 640px; overflow: hidden; margin: 0 auto; position: relative; visibility:hidden; background:#fff url(/public/static/diy/images/diy_bg.png) 0 top no-repeat; background-size:100% auto;}
.main.show{ visibility:visible;}

/*头部*/
.topbar{ display:none;}
.topbar .back-btn,.topbar .share-btn{ width:40px; height:40px;margin-top:5px;}
.topbar .back-btn{background:url(/public/static/diy/images/minback.png) no-repeat center center; background-size:10px auto; display:none;}

/*loading*/
.zhezhaoLayer { position:absolute; top: 0; left:0; z-index: 101; width: 100%;height: 100%; background:rgba(0,0,0,.2);}
.loadingTip{color: #fff;text-align:center; padding: 10px 0; position: absolute; text-align: center; top: 45%; width: 100%; z-index: 111;}
.loadingTip i:nth-child(1){ animation:loading .6s infinite;-webkit-animation:loading .8s infinite;}
.loadingTip i:nth-child(2){ animation:loading .6s .15s infinite;-webkit-animation:loading .6s .15s infinite}
.loadingTip i:nth-child(3){ animation:loading .6s .3s infinite;-webkit-animation:loading .6s .3s infinite}
.loadingTip i:nth-child(4){ animation:loading .6s .45s infinite;-webkit-animation:loading .6s .45s infinite}
.loadingTip i:nth-child(5){ animation:loading .6s .6s infinite;-webkit-animation:loading .6s .6s infinite}
@keyframes loading{
	0%{color:#21D0FD}	
	100%{ color:#fff;}
}
@-webkit-keyframes loading{
	0%{color:#21D0FD}	
	100%{ color:#fff;}
} 

/*prompt*/
.prompt { position:absolute; width: 100%; max-width: 640px; height: 280px; bottom: 0; overflow: hidden; background: #fff; z-index:999; transform: translateY(250px); -webkit-transform: translateY(250px); transition: all .3s; -webkit-transition: all .3s; }
.prompt.show { transform: translateY(0); -webkit-transform: translateY(0); }
.prompt .tit { width: 90%; padding: 20px 0 10px; margin: 0 auto; font-size: 14px; }
.prompt input{border: 1px solid #ddd; border-bottom:0;box-sizing: border-box;display: block;margin: 0 auto;padding: 8px; font-size:14px;width: 90%; border-radius:5px 5px 0 0;}
.prompt textarea { display: block; width: 90%; resize: none; margin: 0; height: 103px; font-size: 14px; border: solid 1px #ddd;display: block; margin: 0 auto; padding: 8px; box-sizing: border-box; border-radius:0 0 5px 5px; }
.prompt .layerbtn { text-align: center; padding-top: 10px; }
.prompt .layerbtn span { display: inline-block; width: 45%; height: 35px; text-align: center; line-height: 35px; border: solid 1px #ddd; margin-left: -1px; background-color: #eee; }
.prompt .layerbtn .ok { border-radius: 0 20px 20px 0; border-left:0;
color: #fff;
border-color: #111;
background-color: #111 ;

}
.prompt .layerbtn .no {border-radius: 20px 0 0 20px;  }

/*展示区*/
.tzBtnBox { font-size: 12px; text-align: center; position:absolute; left:10px; top:50px;}
.tzBtnBox span { margin:6px 0; display: block; box-sizing: border-box; border: solid 1px #8c8c8c; color: #666; width: 52px; line-height:50px; text-align: center; height: 52px; border-radius: 50%; transition: all .2s; overflow:hidden;}
.tzBtnBox span.cur {background:#111; color:#fff; border:#111;}
.showArea {height:calc(100% - 296px); margin:30px 0 0 0;text-align:center; }
.showArea img { max-width:95%; max-height: 95%; margin-top:5%; margin:auto; display:inline; }

/*底部条*/
.bottomBar{ height:45px; line-height:45px; background:#fff; border-top:solid 1px #e6e6e6; padding-left:15px; position:relative; z-index:2;}
.bottomBar .downOrder{ font-size:14px; color:#fff; background:#111; display:block; width:120px; height:45px; text-align:center;}
.bottomBar .saveWorks{ font-size:14px; color:#e66800; background:#f5f5f5; display:block; width:120px; height:45px; text-align:center; border-left:solid 1px #e6e6e6;}

/*组件菜单*/
.opt{ position:relative; z-index:2;}
.opt .minTit { padding:0 0 5px 0; clear: both; color: #999; font-size:12px; }
.opt .scrollBox { height: 280px; overflow-x: hidden; overflow-y: auto; }
.opt .catBox { height:320px; background: rgba(255,255,255,0.9); box-shadow: 0 0 20px #a8adb2; overflow:hidden;}

