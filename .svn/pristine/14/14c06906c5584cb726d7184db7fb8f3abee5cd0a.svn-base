.diy_icon1{animation:twinkle1 8s infinite;-moz-animation:twinkle1 8s infinite;-webkit-animation:twinkle1 8s infinite;-o-animation:twinkle1 8s infinite;}

.diy_icon2{animation:twinkle2 6s 3s infinite;-moz-animation:twinkle2 6s 3s infinite;-webkit-animation:twinkle2 6s 3s infinite;-o-animation:twinkle2 6s 3s infinite;}

.diy_icon3{animation:twinkle2 8s 10s infinite;-moz-animation:twinkle2 8s 10s infinite;-webkit-animation:twinkle2 8s 10s infinite;-o-animation:twinkle2 8s 10s infinite;}


.diy_icon1{position:fixed; z-index:-1; width:100%; height:100%; background:url(../images/diy_icon1.png) center center no-repeat; opacity:.2;filter:alpha(opacity=20);}

.diy_icon2{position:fixed; z-index:-10; width:100%; height:100%; background:url(../images/diy_icon2.png) center center no-repeat;opacity:.04;filter:alpha(opacity=40);}

.diy_icon3{position:fixed; z-index:-20; width:100%; height:100%; background:url(../images/diy_icon3.png) center center no-repeat;opacity:.06;filter:alpha(opacity=60);}


