.cpxq_left li i {background:url(../images/diy_con.png) no-repeat; width:30px; height:30px; display:block; margin:12px auto 2px auto;}
.cpxq_left li i.bg2 {background-position:0 -30px;}
.cpxq_left li i.bg3 {background-position:0 -60px;}
.cpxq_left li i.bg4 {background-position:0 -90px;}
.cpxq_left li i.bg5 {background-position:0 -120px;}
.cpxq_left li .xjt { width:7px; height:4px; display:block; position:absolute; left:50%; bottom:-10px; margin-left:-4px; background:url(../images/xjt_1.png) left top no-repeat;}
.cpxq_left li .yjt {width:8px; height:13px; background:url(../images/xjt_2.png) left top no-repeat; position:absolute; right:-37px; top:50%; margin-top:-4px; display:none;}
.cpxq_left li.on .yjt { display:block;}

.cpxq_left li.on i.bg1 {background-position:-30px 0px;}
.cpxq_left li.on i.bg2 {background-position:-30px -30px;}
.cpxq_left li.on i.bg3 {background-position:-30px -60px;}
.cpxq_left li.on i.bg4 {background-position:-30px -90px;}
.cpxq_left li.on i.bg5 {background-position:-30px -120px;}

.cpxq_left li img {display:block; margin:0 auto; width:30px; height:30px; margin-top:9px; opacity:0.5;}
.cpxq_left li.on {background:#e66800; padding:0; color:#fff; border:0; padding:1px;}
.cpxq_left li.on img {opacity:1;}

