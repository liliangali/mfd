.time-bg #amount{border:0;background-color:#fff;font-weight:bold;font-size:14px;color:#e66800; position:absolute; top:0; left:400px;}
.time-bg #slider-range{width:60%; cursor:pointer; position:absolute; top:5px; left:80px;}
.ui-slider {position:relative;text-align:left;}
.ui-slider .ui-slider-handle {position:absolute;z-index:2;width:14px;height:14px;cursor:move;}
.ui-slider .ui-slider-range {position:absolute;z-index:1;display:block;border:0;background-position:0 0;}
.ui-slider .ui-slider-handle,.ui-slider .ui-slider-range{filter:inherit;}
.ui-slider-horizontal {height:1em;}
.ui-slider-horizontal .ui-slider-handle {top:-4px;margin-left:-.6em;}
.ui-slider-horizontal .ui-slider-range {top:0;height:100%;}
.ui-widget-content{background:#eee 50% top repeat-x; color:#333333; height:10px;}
.ui-widget-header{background:#e66800 50% 50% repeat-x;}
