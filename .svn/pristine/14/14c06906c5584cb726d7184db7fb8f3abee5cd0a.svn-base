25%{opacity:.3;}
50%{opacity:0;}
100%{opacity:.2;}}@-moz-keyframes twinkle1{0%{opacity:.2;}
25%{opacity:.3;}
50%{opacity:0;}
100%{opacity:.2;}}@-webkit-keyframes twinkle1{0%{opacity:.2;}
25%{opacity:.3;}
50%{opacity:0;}
100%{opacity:.2;}}@-o-keyframes twinkle1{0%{opacity:.2;}
25%{opacity:.3;}
50%{opacity:0;}
100%{opacity:.2;}}@keyframes twinkle2{0%{opacity:.05;}
50%{opacity:.2;}
100%{opacity:.05;}}@-moz-keyframes twinkle2{0%{opacity:.05;}
