{include file=../header-new.html} 

<style>
body { padding: 0; margin: 0; font-size: 14px; background: #fff; color: #010101; font-family: "微软雅黑", "Arial"; min-width: 320px; -webkit-text-size-adjust: none; }
h1, h2, h3, h4, h5, h6, p, form, div, dl, dd, dt, ul, li, img { padding: 0; margin: 0; list-style-type: none; border: 0; font-weight: normal;-webkit-tap-highlight-color:rgba(0,0,0,0) }
input, textarea, select { outline: none; list-style-type: none; border: none; padding: 0; margin: 0; background: none; resize:none;}
img, fieldset { border: 0; }
input, img { vertical-align: middle; font-family: 'microsoft yahei', Verdana, Arial, Helvetica, sans-serif; }
ul, ol { list-style: none; }
em, address, i { font-style: normal; }
a { color: #000; text-decoration: none; }
body a { outline: none; blr:expression(this.onFocus=this.blur());}
.clearfix:after { content: "."; height: 0px; line-height: 0px; overflow: hidden; clear: both; display: block; visibility: hidden; }
.clearfix { zoom: 1; }
.jj_box { display: box; display: -webkit-box; display: -moz-box; orient: horizontal; width: 100%; }
.jj_1 { box-flex: 1; -webkit-box-flex: 1; -moz-box-flex: 1; display: block; }
input[type="button"], input[type="submit"], input[type="reset"], input[type="text"], input[type="password"],select,textarea{-webkit-appearance: none;}
:-moz-placeholder { /* Mozilla Firefox 4 to 18 */ color: #999; opacity: 10;}
::-moz-placeholder { /* Mozilla Firefox 19+ */color: #999; opacity: 10;}
input:-ms-input-placeholder,textarea:-ms-input-placeholder {color: #999; opacity: 10;}
input::-webkit-input-placeholder,textarea::-webkit-input-placeholder { color: #999; opacity: 10;}
.main {margin: 0 auto; max-width: 640px; min-width: 320px; width: 100%; overflow:hidden;}

.yhjbg { background:url(data:image/png;base64,) left top no-repeat; height:96px; background-size:100% 100%; margin:0 20px;}
.yhjbg .je { font-size:45px; color:#e66800; font-family:Arial; height:80px; overflow:hidden; line-height:80px; width:80px; text-align:center; padding-left:5px;}
.yhjbg .mdsky { margin-top:17px;}
.yhjbg .mdsky h1 { font-size:21px; color:#995823; line-height:24px; height:24px; overflow:hidden;}
.yhjbg .mdsky h2 { font-size:11px; color:#995823; line-height:20px; height:20px; overflow:hidden;}
.yhjbg .mdsky h2 span { color:#24bbc2;}
.yhjbg .ljlq {display:block;}
</style>


</head>

<body>

<div class="main" style="margin-top:20px;">
{if $newpromotions}
{foreach from=$newpromotions item=news}
  <div class="yhjbg">
  <a  class="ljlq" data-id="{$news.id}">
   <div class="jj_box">
    <p class="je">{$news.yhcase_value2}</p>
    <div class="mdsky jj_1">
      <h1>元优惠券</h1>
      {if $news.yhcase_value}
      <h2>满<span>{$news.yhcase_value}</span>元即可使用</h2>
      {else}
       <h2>无门槛优惠券</h2>
      {/if}
    </div>
    </div>
    </a>
  </div>
  {/foreach}
 {/if}
</div>
{include file=footer.html}
<script src="/public/static/pc/js/jquery-1.9.1.min.js"></script>
<script src="/public/global/luck/pc/luck.js"></script>
<link rel="stylesheet" type="text/css" href="/public/static/layer_mobile/need/layer.css">
<script type="text/javascript" src="/public/static/layer_mobile/layer.js"></script>
<script>
$('.ljlq').click(function(){
	var id=$(this).attr('data-id')
	 $.ajax({
	         url:'/quan-addquan.html',
	         type:'POST',
	         data:"id="+id,
	         dataType: "json",
	         async: false,
	     success:function(res){
	     
	     if(!res.done)
	     {
	    	 luck.open({content:res.msg,time:1500})
	     }
	     else
	    {
	    	 luck.open({content:"领取成功",time:1500})
	      }	
	     		},
	     })
})
</script>
</body>
</html>
