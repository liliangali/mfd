.m_footer { width:1200px; margin:0 auto; border-top:1px solid #dfdfdf; text-align:center;}
.footer .bottom .logo { width: 240px; height: 58px; margin-top:5px; background: url(../img/bg.png) no-repeat 0 -244px; }
.footer .bottom .p1 { margin-top:30px; height:20px; line-height:20px;}
.footer .bottom .p1 span { padding:0 10px; color:#e2e2e2;}
.footer .bottom .p1 a { color:#333;}
.footer .bottom .p1 a:hover {color:#fb4900;}
.footer .bottom .p2 { margin-top:6px; height:20px; line-height:20px;}
.footer .bottom .p2 span { padding-left:18px;}
.footer .share { margin-top: 10px; overflow: hidden; }
.footer .share span { float: left; width: 29px; height: 29px; margin-right: 9px; cursor: pointer; background: url(../img/bg.png) no-repeat 0 -103px; transition: all ease .3s; }
.footer .share .sina:hover { background-position: 0 -137px; }
.footer .share .qzone { background-position: -37px -103px; }
.footer .share .qzone:hover { background-position: -37px -137px; }
.footer .share .weixin { background-position: -74px -103px; }
.footer .share .weixin:hover { background-position: -74px -137px; }
.footer .share .qqwb { background-position: -111px -103px; }
.footer .share .qqwb:hover { background-position: -111px -137px; }

.footer .qrcode { height:100px; background: url(../img/qrcode.png) no-repeat; background-size:74px 72px; padding-left:80px; clear: both; color: #fff; margin-top:10px; }
.footer .qrcode a { color: #ccc; }
.footer .qrcode a { width:132px; height:33px; background:url(../img/icon_2.png) left top no-repeat; display:block; transition: all 0.5s ease 0s;}
.footer .qrcode .Iphome { background-position:0 -72px; margin-bottom:6px;}
.footer .qrcode .Iphome:hover {background-position:0 -111px;}
.footer .qrcode .Android { background-position:0 -152px;}
.footer .qrcode .Android:hover {background-position:0 -191px;}
/*--底部/E--*/

/*--简约底部/S--*/
.footer2{ background:url(../img/footerbg.jpg) repeat-x #363636; height:100px; margin-top:30px; text-align:center; padding-top:50px;}
.footer2 p{ margin-bottom:15px;}
.footer2 span{ padding:0 10px;}
.footer2 a{ color:#fff;}
.footer2 a:hover{ text-decoration:underline;}
/*--简约底部/E--*/

/*--分页/S--*/
.page_box { overflow:hidden;}
.page {position:relative; left:50%; float:left; font-size:14px;}
.page a, .page span {position:relative; right:50%;}
.page a { display:block; float:left; padding:0 18px; margin-right:5px; display:inline; height:36px; background:#fff; border-radius:5px; border:1px solid #ddd; line-height:36px; text-align:center; color:#454545;}
.page a:hover{ background:#f2f2f2; box-shadow:0 0 5px #ccc;}
.page span {display:block; float:left; padding:0 18px; margin-right:5px; display:inline; height:36px; background:#fff; border-radius:5px; border:1px solid #ddd; line-height:36px; text-align:center; color:#454545;}
.page .on,.page .on:hover,.page .page_hover,.page .page_hover:hover {background:#a40001; border:1px solid #a40001; color:#fff;}
.page input { width:46px; height:22px; line-height:22px; border:1px solid #ddd; border-radius:5px; margin:0 8px; text-align:center;}
.page2{ position:relative; left:50%; float:left; font-size:18px;}
.page2 a,.page2 span,.page2 em{ position:relative; right:50%; float:left; padding:0 10px;display:inline;}
.page2 a.on{ color:#a40001;}
.page2 .prev,.page2 .next{ width:7px; height:13px; margin:6px 5px; background:url(../img/step.gif) no-repeat -33px -132px; padding:0; cursor:pointer;}
.page2 .next{ background-position:-40px -132px;}
.page2 .cur{ opacity:0.5;filter:alpha(opacity=50)}
/*--分页/E--*/
