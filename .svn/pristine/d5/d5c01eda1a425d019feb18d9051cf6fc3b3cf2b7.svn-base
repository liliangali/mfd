@charset "utf-8";
/*--全局/S--*/
body { margin: 0; padding: 0; background: #FFF; color:#666; font-size:12px; line-height:none;  font-family:微软雅黑, 'Arial, Helvetica, sans-serif';min-width:1200px; outline:0;}
div, form, ul, ol, li, span, p, dl, dt, dd, img, i, em, input, textarea, h1, h2, h3, h4, h5, h6 { margin: 0; padding: 0; border:0; outline: 0;}
ul, ol, li { list-style: none }
em, i { font-style: normal; }
a { color: #535353; text-decoration: none; outline: 0 }
.light, .light a, a:hover { color: #fb4900; text-decoration:none; }
.fl { float: left; }
.fr { float: right; }
.clearfix:after {content:"."; height:0px; line-height:0px; overflow:hidden; clear:both; display:block; visibility:hidden;}
.clearfix{zoom:1;}
.area{overflow:hidden;}
/*--全局/E--*/

/*--字体颜色/S--*/
.red,.red a{ color:#9e0000;}
.grey,.grey a{ color:#999;}
.green,.green a{ color:green;}
/*--字体颜色/E--*/


/*--带尖角号的连接按钮/S--*/
.btn-angle .link { float: left; }
.btn-angle .angle { float: left; width: 9px; height: 5px; background: url(../img/bg.png) no-repeat -162px -158px; margin:7px 0 0 6px; transition: all ease .3s; -moz-transition: all ease .3s; -ms-transition: all ease .3s; -o-transition: all ease .3s; -webkit-transition: all ease .3s; }
.btn-angle:hover .angle, .btn-angle-hover .angle { transform: rotate(180deg); -moz-transform: rotate(180deg); -ms-transform: rotate(180deg); -o-transform: rotate(180deg); -webkit-transform: rotate(180deg); }
/*--带尖角号的连接按钮/E--*/

/*--固定漂浮导航/S--*/
.fixedNav{ width:50px; position:fixed; z-index:10; top:220px; right:50%; margin-right:620px;}
.fixedNav li{ padding-top:2px; position:relative;}
.fixedNav .cur a{ background:#a40001; color:#fff;}
.fixedNav .cur i{ position:absolute; right:-5px; bottom:0; width:0; height:0; overflow:hidden; line-height:0; border-style:solid; border-color:transparent  transparent  #a40001 #a40001; border-width:0 5px 5px;}
.fixedNav a{ display:block; height:50px; line-height:50px; text-align:center; background:#d7d7d7;}
@media screen and (max-width:1360px){
	.fixedNav{left:0;}
}
/*--固定漂浮导航/E--*/

/*--右下方漂浮导航/S--*/
