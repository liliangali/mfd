body, div, p, ul, ol, li, nav, footer, dl, dt, dd, h1, h2, h3, h4, h5, h6, form, img { padding: 0; margin: 0; }
body { min-width: 320px; margin: 0 auto; font: normal 18px/1.5 helvetica, verdana, san-serif; background: #fafafa; overflow: hidden; height:100%;}
ul, ol, li { list-style: none; }
* {	-webkit-tap-highlight-color: rgba(255,255,255,0); outline:none;}
img { vertical-align: middle; display: block; border: none; }
a { text-decoration: none; }
.fl { float: left; }
.fr { float: right; }
.zhezhaoLayer { position: fixed; top: 0;left:0; z-index: 101; width: 100%;height: 100%; background:#000; opacity: 0.2; }
.hide { display: none; }

/*头部*/
.topbar .back-btn,.topbar .share-btn{ width:40px; height:40px;margin-top:5px;}
.topbar .back-btn{background:url(/public/static/diy/wapstatic/images/minback.png) no-repeat center center; background-size:10px auto;}
.topbar .share-btn{background:url(/public/static/diy/wapstatic/images/share-btn.png) no-repeat center center; background-size:20px auto;}

/*loading*/
.loadingTip{color: #fff;text-align:center; padding: 10px 0; position: absolute; text-align: center; top: 45%; width: 100%; z-index: 111;}
.loadingTip i:nth-child(1){ animation:loading .6s infinite;-webkit-animation:loading .8s infinite;}
.loadingTip i:nth-child(2){ animation:loading .6s .15s infinite;-webkit-animation:loading .6s .15s infinite}
.loadingTip i:nth-child(3){ animation:loading .6s .3s infinite;-webkit-animation:loading .6s .3s infinite}
.loadingTip i:nth-child(4){ animation:loading .6s .45s infinite;-webkit-animation:loading .6s .45s infinite}
.loadingTip i:nth-child(5){ animation:loading .6s .6s infinite;-webkit-animation:loading .6s .6s infinite}
@keyframes loading{
	0%{color:#21D0FD}	
	100%{ color:#fff;}
}
@-webkit-keyframes loading{
	0%{color:#21D0FD}	
	100%{ color:#fff;}
}
/*prompt*/
.prompt { position: fixed; width: 100%; max-width: 640px; height: 280px; bottom: 0; overflow: hidden; background: #fff; z-index:999; transform: translateY(250px); -webkit-transform: translateY(250px); transition: all .3s; -webkit-transition: all .3s; }
.prompt.show { transform: translateY(0); -webkit-transform: translateY(0); }
.prompt .tit { width: 90%; padding: 20px 0 10px; margin: 0 auto; font-size: 14px; }
.prompt input{border: 1px solid #ddd; border-bottom:0;box-sizing: border-box;display: block;margin: 0 auto;padding: 8px; font-size:14px;width: 90%; border-radius:5px 5px 0 0;}
.prompt textarea { display: block; width: 90%; resize: none; margin: 0; height: 103px; font-size: 14px; border: solid 1px #ddd;display: block; margin: 0 auto; padding: 8px; box-sizing: border-box; border-radius:0 0 5px 5px; }
.prompt .layerbtn { text-align: center; padding-top: 10px; }
.prompt .layerbtn span { display: inline-block; width: 45%; height: 35px; text-align: center; line-height: 35px; border: solid 1px #ddd; margin-left: -1px; background-color: #eee; background-image: -moz-linear-gradient(#fff, #eee); background-image: -webkit-linear-gradient(#fff, #eee);background-image: linear-gradient(#fff, #eee); }
.prompt .layerbtn .ok { border-radius: 20px 0 0 20px;
color: #fff;
border-color: #21d0fd;
background-color: #21d0fd ;
background-image: -moz-linear-gradient(#4FDBFF, #21d0fd );
background-image: -webkit-linear-gradient(#4FDBFF, #21d0fd );
background-image: linear-gradient(#4FDBFF, #21d0fd );
}
.prompt .layerbtn .no { border-radius: 0 20px 20px 0; border-left:0; }

/*颜色*/
.orange { color: #f50;}

/*布局*/
.main { width: 100%; max-width: 640px; overflow: hidden; margin: 0 auto; position: relative; opacity:0;}
.main.show{ opacity:1;}

/*展示区*/
.tzBtnBox { font-size: 12px; text-align: center; position:absolute; left:10px; top:50px;}
.tzBtnBox span { margin:6px 0; display: block; box-sizing: border-box; border: solid 2px #e6e6e6; color: #717171; width: 50px; line-height: 46px; text-align: center; height: 50px; border-radius: 50%; transition: all .2s; overflow:hidden;}
.tzBtnBox span.cur {border-color: #21d0fd; color:#21d0fd; }
.showArea {height:calc(100% - 296px); margin:30px 0;text-align:center; }
.showArea img { max-width: 100%; max-height: 100%; display:inline; }
/*底部条*/
.bottomBar{ height:45px; line-height:45px; background:#fafafa; border-top:solid 1px #e6e6e6; padding:0 15px; position:relative; z-index:2;}
.bottomBar .downOrder{ font-size:16px; margin-left:15%; color:#21d0fd}
.bottomBar .saveWorks{ font-size:14px; color:#666;}

/*组件菜单*/
.opt{ position:relative; z-index:2;}
.opt .minTit { padding: 5px 0; clear: both; color: #999; font-size:12px; }
.opt .scrollBox { height: 145px; overflow-x: hidden; overflow-y: auto; }
.opt .catBox { height: 190px; background: rgba(255,255,255,0.9); box-shadow: 0 0 20px #a8adb2; overflow:hidden;}
.opt .cat1 { text-align: center; overflow: hidden; }
.opt .cat1 li { width: 25%; height:75px; float: left; border-top:solid 1px #e6e6e6; border-left:solid 1px #e6e6e6; margin:-1px 0 0 -1px; padding-top:22px;color: #666; font-size: 12px; }
.opt .cat1 .nav-ico{display: block; margin: 0 auto; background: url(../images/100.png) no-repeat;width: 35px; height: 35px; background-size:auto 30px; opacity:.9;}
.opt .cat1 .nav-ks .nav-ico{ background-position:-70px 0;}
.opt .cat1 .nav-ks0003 .nav-ico{ background:url(../images/ico.png) 3px 0 no-repeat; background-size:auto 30px;}
.opt .cat1 .nav-ks0004 .nav-ico{ background:url(../images/ico.png) -158px 0 no-repeat; background-size:auto 30px;}
.opt .cat1 .nav-ks0005 .nav-ico{ background:url(../images/ico.png) -37px 0 no-repeat; background-size:auto 30px;}
.opt .cat1 .nav-ks0007 .nav-ico{ background:url(../images/ico.png) -76px 0 no-repeat; background-size:auto 30px;}
.opt .cat1 .nav-wg .nav-ico{ background-position: -102px 0} 
.opt .cat1 .nav-kd .nav-ico{ background-position: -153px 0}
.opt .cat1 .nav-kx .nav-ico{ background-position: -249px 0}
.opt .cat1 .nav-lc .nav-ico{ background-position: -198px 0}
.opt .cat1 .nav-gd .nav-ico{ background-position: -308px 0}
.opt .cat1 .nav-embroidery .nav-ico{ background-position:-358px 0;}
.opt .cat1 .nav-xj .nav-ico{ background-position:-411px 0;}
.opt .cat2 { display: none; }
.opt .cat2 .loading { text-align: center; font-size: 14px; color: #21d0fd; line-height: 90px; }
.opt .cat2 .titBar { height: 45px; line-height: 45px; padding-right: 15px; overflow: hidden; }
.opt .cat2 .titBar .tit { background: url(../images/minback.png) no-repeat 13px center; background-size: auto 15px; padding: 0 30px; }
.opt .cat2 .titBar .del { margin-left: 10px; }
.opt .cat2 .itemList { margin:10px 15px 0; overflow: hidden; padding-bottom: 20px; }
.opt .cat2 .itemList .status { text-align: center; line-height: 70px; color: #aaa; font-size:14px; }
.opt .cat2 .itemList ul{ margin-left:-2%; clear:both;}
.opt .cat2 .itemList li { width: 23%; float: left; position: relative; text-align: center; font-size:8px; margin:0 0 35px 2%;}
#opt-kx .itemList>div:first-child img{ background:none; border:none; width:80%;}
.opt .cat2 .itemList .kxSelect:after{ content:'点击修改'; position:absolute; left:0; top:50%; margin-top:-10px; width:100%; height:20px; line-height:20px; color:#999; text-align:center;}
.opt .cat2 .itemList li p { height: 25px;line-height: 13px; position: absolute; bottom: -30px; left:10%; width:80%; color: #333; white-space:nowrap; overflow:hidden; text-overflow:ellipsis;}
.opt .cat2 .itemList li img { margin: 0 auto; border-radius:3px; width:100%; height: auto; box-sizing: border-box; border: solid 1px #e6e6e6; background: #fff; }
.opt .cat2 .itemList li.cur:after { content: ''; display: block; width: 36px; height: 36px; position: absolute; left: 50%; top: 50%; margin: -18px 0 0 -18px; background: url(../images/dui.png) no-repeat center center #21d0fd; background-size: 60% auto; border-radius: 50%; overflow: hidden;pointer-events:none;}
.opt .cat2 .itemList li .tit { position: absolute; left: 0; bottom: 10px; width: 100%; text-align: center; font-size: 12px; }
.opt .subCat{ text-align:center; margin:0 15px;font-size: 14px;}
.opt .cat2-1 { width:100%; border-radius:3px; overflow:hidden;text-align: center; border:solid 1px #e6e6e6;display: box; display: -webkit-box; display: -moz-box; orient: horizontal;}
.opt .cat2-1 li { line-height:30px;border-left: solid 1px #e6e6e6; border-top:solid 1px #e6e6e6; margin:-1px 0 0 -1px; background: #fff;box-flex: 1; -webkit-box-flex: 1; -moz-box-flex: 1; }
.opt .cat2-1 .cur { background: #21d0fd; color: #fff; }
.opt .weitiao { color: #fff; line-height: 30px;}
.opt .weitiao span { background: #cee1ee;text-align: center;  border-radius: 5px; display: inline-block; margin: 1.5%; width: 22%; }
.opt .weitiao .cur { background: #21d0fd; }
.kxSelect select{ position:absolute; left:0; top:0; width:100%; height:100%; opacity:0;}

.lineLayer{ width:100%; max-height:640px; height:236px; position:absolute; z-index:2; bottom:0; background:#fff;}
.lineLayer button{ background:#f6f6f6; color:#21d0fd; font-size:16px; border:0; border-top:solid 1px #e6e6e6; height:40px; width:100%;}
.popLineColor{ height: 182px;margin-left: -2%;margin-top: 15px;overflow: auto;padding: 0 15px;}
.popLineColor li{ float:left; width:25%;}
.popLineColor li { width: 23%; float: left; position: relative; text-align: center; font-size: 10px; margin:0 0 35px 2%;}
.popLineColor li.cur:after { content: ''; display: block; width: 36px; height: 36px; position: absolute; left: 50%; top: 50%; margin: -18px 0 0 -18px; background: url(../images/dui.png) no-repeat center center #21d0fd; background-size: 60% auto; border-radius: 50%; overflow: hidden;pointer-events:none;}
.popLineColor img { margin: 0 auto; width:100%; height: auto; box-sizing: border-box; border: solid 1px #e6e6e6; background: #fff; border-radius:3px;}
.popLineColor li p { height: 25px;line-height: 13px; position: absolute; bottom: -30px; left:10%; width:80%; color: #333; white-space:nowrap; overflow:hidden; text-overflow:ellipsis;}
/*组件tag标签*/
.itemList li .ico { position: absolute; left: 0; top: 0; height: 24px; line-height: 24px; padding: 0 10px; color: #fff; font-size:12px; font-style:normal; border-radius:3px 0 3px 0;}
.itemList li .ico.green { background: #39d37c;}
.itemList li .ico.blue { background: #0ae;}
.itemList li .ico.orange { background: #f50;}
.ds-1{background: url(/public/static/diy/wapstatic/images/yxj.png) no-repeat; background-size:100% auto;height: 100%;left: 0;position: absolute;top: 0;width: 100%;z-index: 1;}
.ds-2{background: url(/public/static/diy/wapstatic/images/jqqd.png) no-repeat; background-size:100% auto;height: 100%;left: 0;position: absolute;top: 0;width: 100%;z-index: 1;}
/*面料*/
#opt-fabric .cat2-2 { font-size: 14px; color: #666;background:#fff;text-align: center;overflow:hidden; padding-top:10px;}
#opt-fabric .cat2-2 span { width: 20%; box-sizing: border-box; display: inline-block; white-space: nowrap; padding:6px 10px; float: left; overflow:hidden; text-overflow:ellipsis;}
#opt-fabric .cat2-2 .cur { color: #21d0fd }
#opt-fabric .itemList li,#opt-lc .itemList li { width: 31.33%;}
#opt-fabric .itemList li img{ border:0;}

/*刺绣*/
.opt .cxCon { margin: 5px 0; }
.opt .cxCon input { width: 100%; height: 40px; padding: 3px;box-sizing: border-box; border: solid 1px #ddd; }
.opt .cxPosition {font-size: 14px; }
.opt .cxPosition p {overflow: hidden; padding: 5px 0 5px 30px; color: #666; }
.opt .cxPosition span { width: 20px; height: 20px; position: relative;border-radius: 50%; float:left; margin-left:-26px; box-sizing: border-box; border: solid 1px #c7d6df; }
.opt .cxPosition .cur span { background: #21d0fd; border: 0; }
.opt .cxPosition .cur span:after { content: ''; display: block; width: 90%; height: 90%; position: absolute; left: 5%; top: 5%; background: url(../images/dui.png) no-repeat center center; background-size: 60% auto; overflow: hidden; }

/*下单弹层*/
.bottomSize{ height:240px; bottom: 0; max-width: 640px; overflow: hidden; position:absolute; z-index: 102; width: 100%; background:#fff; transform: translate3d(0,240px,0); -webkit-transform: translate3d(0,240px,0); transition: all .3s; -webkit-transition: all .3s; }
.bottomSize.show { transform: translate3d(0,0,0); -webkit-transform: translate3d(0,0,0) }
.bottomSize .tit{ font-size:14px; color:#999; padding:25px 15px 0}
.bottomSize .close{ width:20px; height:20px; background:url(../images/close.png) no-repeat center center; background-size:15px auto; position:absolute;top:15px; right:15px;}
.bottomSize .button{ display:block; border:solid 1px #ccc; text-align:right; padding:12px 20px; font-size:16px; color:#333; margin:25px 15px 0; background:url(../images/size.png) no-repeat; background-size:40px auto;}
.bottomSize .base{ background-position:10px 8px;}
.bottomSize .amount{ background-position:10px -50px;}
.bottomSize .base:active{ background-color:#0ae;background-position:10px -99px;color:#fff;border-color:#0ae;}
.bottomSize .amount:active{background-color:#0ae;background-position:10px -156px;color:#fff;border-color:#0ae;}

/*尺码表*/
.sizeTable .tabBar { line-height:30px; overflow:hidden; margin:10px 3%;}
.sizeTable .tabBar li { float: left; border-bottom: solid 2px rgba(255,255,255,0); padding: 0 20px; }
.sizeTable .tabBar .cur { color: #21d0fd; border-color: #21d0fd; }
.sizeTable .itemList {overflow: hidden; line-height: 30px; color: #666; font-size: 14px; }
.sizeTable .itemList li { float: left; width:27.3%; margin:1.5% 3%; text-align: center; background: #fff; border-radius: 5px; border: solid 1px #d5d5d5; box-sizing: border-box; }
.sizeTable .itemList .cur { border-color: #21d0fd; color: #21d0fd; }
.sizeTable .loading { text-align: center; line-height: 140px; }
.sizeTable .scrollBox { overflow-x: hidden; overflow-y: auto;height:257px;height:-webkit-calc(100% - 143px) }

/*尺码助手*/
.sizeReferTable table{ border-collapse:collapse; font-size:12px; color:#999}
.sizeReferTable td,.sizeReferTable th{ border:solid 1px #eee; text-align:center; padding:3px;}
.sizeReferTable th{ white-space:nowrap; background:#f8f8f8; font-weight:normal; color:#333;}
.sizeReferTable .scrollBox div{ width:560px; padding:20px;}
.sizeReferTable .scrollBox{height:353px;overflow:auto;}
.sizeReferTable .loading { line-height: 140px; font-size:14px;}
.sizeReferTable h2 {font-size: 16px;font-weight: normal;padding: 0 0 10px;}

/*弹层公共*/
.popLayer{ width:100%; height:100%; position:absolute;z-index:102; right:0; bottom:0; background:#fff; height:400px;
transform:translate3d(0,400px,0);
-webkit-transform:translate3d(0,400px,0);
transition:all .5s;
-webkit-transition:all .5s;}
.popLayer.show{transform:translate3d(0,0,0);-webkit-transform:translate3d(0,0,0);}
.popLayer .head{ text-align:center; height:45px; line-height:45px; border-bottom:solid 1px #e6e6e6;}
.popLayer .head .back{ position:absolute; right:0; top:0; width:45px; height:45px; background:url(../images/close.png) no-repeat center center; background-size:15px auto;}
.popLayer .head a{ position:absolute; left:20px; top:0; font-size:14px; color:#717171;}
.popLayer .downBtn{width:100%; height:45px; border:0; background:#f6f6f6; border-top:solid 1px #e6e6e6; color:#21d0fd; font-size:16px;}

/*分享*/
.cotteshare{ height:150px;transform:translate3d(0,150px,0);-webkit-transform:translate3d(0,150px,0);transition:all .3s;-webkit-transition:all .3s;}
.cotteshare .btnBox{ text-align:center; padding:20px 0;}
.cotteshare .btnBox a{ display:inline-block;width:50px; height:50px; background:url(/public/static/diy/wapstatic/images/share.gif) no-repeat; background-size:50px auto; margin:0 10px; vertical-align:middle;}
.cotteshare .btnBox .cotte_qzone{ background-position:0 -50px;}
.cotteshare .btnBox .cotte_tsina{ background-position:0 -150px;}
.cotteshare .btnBox .cotte_sqq{ background-position:0 -100px;}
.cotteshare .btnBox .cotte_tqq{ background-position:0 0;}

/*登录层*/
.loginPop{ height:250px;transform:translate3d(0,250px,0);-webkit-transform:translate3d(0,250px,0);transition:all .3s;-webkit-transition:all .3s;}
.loginPop.show{ box-shadow:0 0 20px #a8adb2;}
.loginPop ul{margin:25px 25px 0;border:solid 1px #ddd; border-radius:5px; overflow:hidden;}
.loginPop li:first-child{ border-bottom:solid 1px  #ddd;}
.loginPop input{ width:100%; padding:0 10px; height:40px; box-sizing:border-box; display:block; margin:0 auto; border:0;}
.loginPop p{ overflow:hidden; font-size:14px; padding:10px 40px; height:31px; line-height:25px;}
.loginPop a{ color:#999;}