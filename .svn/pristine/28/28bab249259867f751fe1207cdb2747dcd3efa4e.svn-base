*{margin: 0;padding: 0;font: inherit;-moz-box-sizing: border-box;-webkit-box-sizing: border-box;box-sizing: border-box;}
html{-webkit-text-size-adjust: 100%;height: 100%;}
body{max-width: 640px;min-width: 320px;margin: 0 auto;padding: 0;font-family: PingFangSC-Regular, Helvetica, "Droid Sans", Arial, sans-serif;font-size:12px; background: #f8f8f8;overflow-scrolling: touch;overflow-x: hidden;color: #666;}
ol,
li,
ul{list-style: none}
i{font-style: normal;display: inline-block;}
img{display: block; max-width: 100%; max-height: 100%; vertical-align: middle;}
a,
div,
header,
span{-webkit-tap-highlight-color: rgba(255, 255, 255, 0)}
.clear:before,
.clear:after{display: table;content: "";line-height: 0;clear: both;}
textarea{outline: none;resize: none;}
input[type="button"],
input[type="submit"],
input[type="reset"],
input[type="text"],
input[type="password"],
textarea{-webkit-appearance: none;}
input,
textarea,
select,
button{outline: none;list-style-type: none;border: none;padding: 0;margin: 0;background: none;resize: none;-moz-box-sizing: border-box;-webkit-box-sizing: border-box;box-sizing: border-box;}
a{color: #666;text-decoration: none;}
/*body的样式*/
.bg{width: 100%;height: 100%;}
.mhome{padding: 0;min-width: 300px;max-width: 640px;margin: 0 auto;}
/*盒内距*/
.c_bs{-moz-box-sizing: border-box;-webkit-box-sizing: border-box;box-sizing: border-box;}
/*清除浮动*/
.clearfix:after{content: "";display: block;height: 0;clear: both;line-height: 0;overflow: hidden;}
.clearfix{*zoom: 1}
/*布局*/
.c_db{display: box;display: -webkit-box;display: -moz-box;}
.c_bf1{-moz-box-flex: 1;-webkit-box-flex: 1;box-flex: 1;}
.c_bf2{-moz-box-flex: 2;-webkit-box-flex: 2;box-flex: 2;}
.c_bf3{-moz-box-flex: 3;-webkit-box-flex: 3;box-flex: 3;}
/*box-align布局_垂直对齐方式*/
.c_bas{-moz-box-align: start;-webkit-box-align: start;-o-box-align: start;box-align: start;/*顶对齐*/}
.c_bac{-moz-box-align: center;-webkit-box-align: center;-o-box-align: center;box-align: center;/*居中*/}
.c_bae{-moz-box-align: end;-webkit-box-align: end;-o-box-align: end;box-align: end;/*底对齐*/}
.c_bast{-moz-box-align: stretch;-webkit-box-align: stretch;-o-box-align: stretch;box-align: stretch;/*拉伸对齐*/}
/*box-pack布局_水平对齐方式*/
.c_bps{-moz-box-pack: start;-webkit-box-pack: start;-o-box-pack: start;box-pack: start;/*左对齐*/}
.c_bpc{-moz-box-pack: center;-webkit-box-pack: center;-o-box-pack: center;box-pack: center;/*居中对齐*/}
.c_bpe{-moz-box-pack: end;-webkit-box-pack: end;-o-box-pack: end;box-pack: end;/*右对齐*/}
.c_bpj{-moz-box-pack: justify;-webkit-box-pack: justify;-o-box-pack: justify;box-pack: justify;/*水平等分父容器宽度*/}
/*box-orient布局_水平还是垂直*/
.c_bov{-moz-box-orient: vertical;-webkit-box-orient: vertical;box-orient: vertical;/*垂直*/}
.c_boh{-moz-box-orient: horizontal;-webkit-box-orient: horizontal;box-orient: horizontal;/*水平*/}
/*box-direction布局_排列顺序*/
.c_bdr{-moz-box-direction: reverse;-webkit-box-direction: reverse;box-direction: reverse;/*设置为反转*/}
/*对齐*/
.c_txl{text-align: left;}
.c_txr{text-align: right;}
.c_txc{text-align: center;}
.fl{float: left;}
.fr{float: right;}
/*箭头、关闭符*/
.c_jtb{border-left:1px solid #999;border-bottom: 1px solid #999; width:8px;height:8px;display: inline-block;transform: rotate(-45deg);-moz-transform: rotate(-45deg);-webkit-transform: rotate(-45deg);position: relative;top: -0.2em;}
.c_jtr{border-left:1px solid #999;border-bottom: 1px solid #999; width:8px;height:8px;display: inline-block;transform: rotate(-135deg);-moz-transform: rotate(-135deg);-webkit-transform: rotate(-135deg);}
.c_jtl{border-left:1px solid #fff;border-bottom: 1px solid #fff; width:8px;height:8px;display: inline-block;transform: rotate(-135deg);-moz-transform: rotate(-135deg);-webkit-transform: rotate(45deg);}
.c_close{width: 1.25em;height: 1.25em;overflow: hidden;position: relative;margin: 1em;transform: rotate(45deg);}
.c_close:after,
.c_close:before{content: '';background: #999;height: 0.09375em;width: 100%;display: block;position: absolute;top: 0.5em;left: 0;}
.c_close:before{transform: rotate(90deg);}
/*间距*/
.mt_075{margin-top: 0.75em;}
.mt_1{margin-top: 1em;}
.mt_175{margin-top: 1.75em;}
.pd625{padding-left: 0.625em;padding-right: 0.625em;}
/*字号*/
.c_f6{font-size: 0.625em;}
.c_f7{font-size: 0.75em;}
.c_f8{font-size: 0.875em;}
.c_f106{font-size: 1.0625em;}
.c_f125{font-size: 1.125em;}
/*颜色*/
.c_c0d{color: #0d0d0d;}
.c_c333{color: #333;}
.c_c999{color: #999;}
.c_cd{color: #d10f17;}
/*按钮*/
.c_btsm{display: inline-block;padding: 0.625em 1em;border: 1px solid #dfdfdf;background: #fff;border-radius: 0.25em;color: #333;}
.c_btim{display: inline-block;padding: 0.625em 1em;border: 1px solid #dfdfdf;background: #151515;border-radius: 0.25em;color: #333;}
.c_btim{display: inline-block;padding: 0.625em 1em;border: 1px solid #dfdfdf;border-radius: 0.25em;color: #333;}
.c_btn{padding: 0.875em 0 0.9em 0;background: #0d0d0d;border-radius: 0.25em;color: #fff;width: 100%;text-align: center;}
.c_btn_b{padding: 0.875em 0 0.9em 0;background: #fff;border-radius: 0.25em;color: #151515;border: 0.125em solid #151515;width: 100%;text-align: center;}
.ft_button{height: 4.625em;}
.ft_fixed{position: fixed;left: 0;bottom: 0;padding: 0.5em 0;width: 100%;background: rgba(255, 255, 255, 0.9);border-top: 1px solid #e5e5e5;}
.ft_fixed div{padding: 0 0.275em;}
.ft_fixed div:first-child{padding-left: 0.625em;}
.ft_fixed div:last-child{padding-right: 0.625em;}
.ft_fixed div span{width: 100%;display: block;}
.upload_btn{width: 4em;height: 4em;border-radius: 0.25em;border: 2px dashed #bcbcbc;display: inline-block;background: url(icons/icon-add-act.png) center 0.75em no-repeat;text-align: center;}
.upload_btn i{font-size: 0.75em;position: relative;top: 2.75em;}
/*表单*/
.form_box{padding: 2.75em 1.5em 0 1.5em;}
.input{width: 100%;line-height: 3.1em;padding: 0 0.625em;margin-bottom: 0.875em;border: 0.0625em solid #bcbcbc;border-radius: 0.375em;}
.button{display: block;background: #151515;border-radius: 0.25em;color: #fff;width: 100%;line-height: 3.175em;text-align: center;}
.textarea{background: #fff;border: 1px solid #bcbcbc;width: 100%;border-radius: 0.25em;padding: 0.625em;}
.yzm_box{width: 100%;font-size: 0.875em;line-height: 3.1em;padding: 0 0.625em;margin-bottom: 0.875em;border: 0.0625em solid #bcbcbc;border-radius: 0.375em;}
.yzm_box div{border-left: 1px solid #dfdfdf;padding: 0 0.375em 0 0.875em;}
.yzm_box p{padding-right: 0.625em;}
.yzm_box p input{width: 100%;}
/*其它*/
.c_bdb{border-bottom: 1px solid #dfdfdf;}
.c_bdt{border-top: 1px solid #dfdfdf;}
.c_pabs{position: absolute;}
.c_prel{position: relative;}
.bg_white{background: #fff;}
.bg_gray{background: #f0f0f0;}
.c_fixed{position: fixed;bottom: 0;left: 0;width: 100%;padding: 0.625em;z-index: 100;}
.c_fixed_t{position: fixed;top: 3.15em;left: 0;width: 100%;z-index: 100;}
.c_space{height: 4em;}
.c_off{width: 1.375em;height: 1.375em;border: 0.0625em solid #ccc;background: #fff;border-radius: 50%;vertical-align: middle;-moz-box-sizing: border-box;-webkit-box-sizing: border-box;box-sizing: border-box;display: inline-block;}
.c_on{display: inline-block;width: 1.375em;height: 1.375em;background: #0d0d0d;border-radius: 50%;position: relative;vertical-align: middle;}
.c_on:after{content: '';display: block;width: 0.625em;height: 0.3em;border-left: 0.125em solid #fff;border-bottom: 0.125em solid #fff;-webkit-transform: rotate(-45deg);position: absolute;left: 0.35em;top: 0.35em;}
/*我的内容*/
.bock1{width: 100%;height: 5px;background-color: #FFFFFF;}
.bock2{width: 100%;height: 10px;background-color: #FFFFFF;}
.bock3{width: 100%;height: 15px;background-color: #FFFFFF;}
.bock4{width: 100%;height: 20px;background-color: #FFFFFF;}
.bockM{width: 100%;height: 55px;background: #FFFFFF;}
.opty{opacity: 0;}
.caret_d{display: inline-block;width: 0;height: 0;vertical-align: top;border-top: 6px solid #9A9A9A;border-right: 6px solid transparent;border-left: 6px solid transparent;content: "";vertical-align: middle;}
.caret_r{display: inline-block;width: 0;height: 0;vertical-align: top;border-left: 6px solid #9A9A9A;border-top: 6px solid transparent;border-bottom: 6px solid transparent;content: "";vertical-align: middle;}
/*轮播图公共样式*/
/*划窗*/
.swiper-container{margin-left: auto;margin-right: auto;position: relative;z-index: 1}
.swiper-container-vertical>.swiper-wrapper{-webkit-box-orient: vertical;-moz-box-orient: vertical;-ms-flex-direction: column;-webkit-flex-direction: column;flex-direction: column}
.swiper-wrapper{position: relative;width: 100%;height: 100%;z-index: 1;display: -webkit-box;display: -moz-box;display: -ms-flexbox;display: -webkit-flex;display: flex;-webkit-transition-property: -webkit-transform;-moz-transition-property: -moz-transform;-o-transition-property: -o-transform;-ms-transition-property: -ms-transform;transition-property: transform;-webkit-box-sizing: content-box;-moz-box-sizing: content-box;box-sizing: content-box}
.swiper-container-android .swiper-slide,
.swiper-wrapper{-webkit-transform: translate3d(0, 0, 0);-moz-transform: translate3d(0, 0, 0);-o-transform: translate(0, 0);-ms-transform: translate3d(0, 0, 0);transform: translate3d(0, 0, 0)}
.swiper-slide{-webkit-flex-shrink: 0;-ms-flex: 0 0 auto;flex-shrink: 0;width: 100%;height: 100%;position: relative}
.swiper-container-cube .swiper-slide-active,
.swiper-container-cube .swiper-slide-next,
.swiper-container-cube .swiper-slide-next+.swiper-slide,
.swiper-container-cube .swiper-slide-prev{pointer-events: auto;visibility: visible}
/*分页器*/
.swiper-pagination{position: absolute;text-align: center;-webkit-transition: .3s;-moz-transition: .3s;-o-transition: .3s;transition: .3s;-webkit-transform: translate3d(0, 0, 0);-ms-transform: translate3d(0, 0, 0);-o-transform: translate3d(0, 0, 0);transform: translate3d(0, 0, 0);z-index: 10}
.swiper-pagination.swiper-pagination-hidden{opacity: 0}
.swiper-container-horizontal>.swiper-pagination-bullets,
.swiper-pagination-custom,
.swiper-pagination-fraction{bottom: 10px;left: 0;width: 100%}
.swiper-pagination-bullet{width: 8px;height: 8px;display: inline-block;border-radius: 100%;background: #000;opacity: .2}
.swiper-container-horizontal>.swiper-pagination-bullets .swiper-pagination-bullet{margin: 0 5px}
.swiper-pagination-bullet-active{opacity: 1;background: #007aff}
.swiper-pagination-white .swiper-pagination-bullet-active{background: #fff}
.swiper-pagination-black .swiper-pagination-bullet-active{background: #000}
/* Demo Styles */
.swiper-container{width: 100%;color: #fff;text-align: center;}
.red-slide{background: #ca4040;}
.orange-slide{background: #ff8604;}
.green-slide{background: #49a430;}
.pink-slide{background: #973e76;}
.swiper-slide .title{font-style: italic;font-size: 42px;margin-top: 80px;margin-bottom: 0;line-height: 45px;}
.pagination{position: absolute;z-index: 20;left: 10px;bottom: 10px;}
.swiper-pagination-switch{display: inline-block;width: 8px;height: 8px;border-radius: 8px;background: #222;margin-right: 5px;opacity: 0.8;border: 1px solid #fff;cursor: pointer;}
.swiper-visible-switch{background: #aaa;}
.swiper-active-switch{background: #fff;}

