{include file="header.html"}
<div id="rightTop">

  <ul class="subnav">
    <li><span>个性化标签功能</span></li>
  </ul>
</div>
<div class="tdare">
  <table width="100%" cellspacing="0" class="dataTable">
    <!--{if $ordergoodslist}-->
    <tr class="tatr1">
      <td width="15%"><span ectype="order_by" fieldname="goods_name">狗狗名称</span></td>
      <td width="20%"><span ectype="order_by" fieldname="is_sale">寄语</span></td>
	  <td width="15%"><span ectype="order_by" fieldname="is_sale">生日</span></td>
	  <td width="30%"><span ectype="order_by" fieldname="is_sale">标签图</span></td>
      <td>审核</td>
	  <td>编辑</td>
    </tr>
    <!--{/if}-->
    <!--{foreach from=$ordergoodslist item=goods}-->
    <tr class="tatr2">
      <td>{$goods.dog_name|escape}</td>
      <td>{$goods.dog_desc|escape}</td>
      <td>{$goods.dog_date|escape}</td>
	  <td><img src="{$goods.style}" style="width:80px;height:80px;"/></td>
	  <td>{if $goods.status =='0' or !$goods.status}
	  未审核
	  {elseif $goods.status =='1'}
	  审核成功
	  {elseif $goods.status =='2'}
	  审核失败
	  {/if}
	  </td>
     <td class="handler">
	 <a href="index.php?app=bannerimg&amp;act=view_img&amp;rec_id={$goods.rec_id}">查看</a>
      </td>
    </tr>
    <!--{foreachelse}-->
    <tr class="no_data info">
      <td colspan="7">{$lang.no_data}</td>
    </tr>
    <!--{/foreach}-->
  </table>
 
  <div id="dataFuncs">
    <div class="pageLinks">{if $ordergoodslist}{include file=page.bottom.html}{/if}</div>

  </div>
  <div class="clear"></div>
</div>
