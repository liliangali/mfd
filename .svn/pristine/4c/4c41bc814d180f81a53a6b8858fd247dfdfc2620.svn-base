/*主题系列头部轮播*/
.bg68comad,.motif_focus,.motif_focus ul,.motif_focus ul li,.banclj a{height:400px; overflow:hidden;}
.motif_focus {width:100%; overflow:hidden; position:relative;}
.motif_focus ul {position:absolute;list-style:none; margin:0px; padding:0px;}
.motif_focus ul li {float:left; width:100%;overflow:hidden; position:relative; background:#000;list-style:none; margin:0px; padding:0px;}
/*.focus ul li div {position:absolute; overflow:hidden;}*/
.banclj a{width:800px;display:block; margin:0 auto;}
.motif_focus .btnBg {position:absolute; width:100%; height:20px; left:0; bottom:0; background:#000; display:none;}
.motif_focus .btn {height: 25px;position: absolute;right:10px;bottom:8px;text-align: right;width: 550px; display:none; }
.motif_focus .btn span {color: #FFFFFF; cursor: pointer;display: inline-block;font: 28px/22px tahoma,Arial,Simsun;height: 25px;margin-left:7px; text-align:center;}
.motif_focus .btn span.on {color: #CC0000;cursor: default;}
.motif_focus .preNext {width:36px; height:70px; position:absolute; top:165px;  cursor:pointer; background-position:-1px -2px;}
.motif_focus .pre {left:15%;}
.motif_focus .next {right:15%; background-position:-44px -2px;}
