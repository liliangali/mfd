@charset "utf-8";
/* CSS Document */

table, textarea { font-size: 12px }
body a { outline: none; blr:expression(this.onFocus=this.blur());}
.fl { float: left; }
.fr { float: right; }
.clearfix:after { content: "."; height: 0px; line-height: 0px; overflow: hidden; clear: both; display: block; visibility: hidden; }
.clearfix { zoom: 1; }
.area { overflow: hidden; }
.clear { clear: both; }
input{-webkit-appearance:none;-webkit-border-radius:0}

/*如何开始*/
.rhks_banner { height:360px !important; background: url(/public/static/pc/images/rhks.jpg) repeat-x center center !important; text-align: center; color: #fff; overflow: hidden; padding: 0 20px;}
.rhks_banner h2 {height:1px; background:#e66800; width:80px; margin:15px auto auto auto;}
.rhks_banner h1 { font: 36px/40px ''; margin-top: 100px; }
.rhks_banner p { max-width: 1060px; margin: 30px auto 0; font: 16px/32px ''; }

.rhks_box {max-width:1072px; margin:0 auto;}
.rhks_box img{ margin-top:20px; max-width:100%;}
.bt_3 {font-size:36px; margin:126px 0 28px 0; height:40px; line-height:40px; overflow:hidden; color:#333; text-align:center;}
.rhks {margin:70px 0 40px 0;}
.bczl .p1 {font-size:22px; line-height:30px; color:#666; text-align:center; margin-bottom:20px;}
.bczl .p2 { line-height:24px; color:#666; font-size:16px; text-align:center; padding:0 5%;}
.bczl .p3 { text-align:center; margin-right:60px;}
.sqcwcyz a {background:#7ebeeb; color:#fff;display:block;font-size:20px; font-weight:500; height:46px; line-height:46px; margin:100px auto; text-align:center; width:300px; transition:all 1s ease 0s;}
.sqcwcyz a:hover { background:#4aacf0;}

