#cnzz_stat_icon_1255125981{ display:none;}
.xubox_setwin .xubox_close0{ width:19px; height:19px;}

/*关于我们*/
.banner { height: 400px; background: url(/public/static/pc/images/other/banner.gif) repeat-x center center; text-align: center; color: #fff; overflow: hidden; padding: 0 20px; }
.banner h1 { font: 36px/40px ''; margin-top:86px; }
.banner p { max-width: 1060px; margin: 30px auto 0; font: 16px/32px ''; }
.txtInfo { max-width: 1050px; margin: 140px auto 0; text-align: center; font: 16px/32px ''; padding: 0 20px; }
.txtInfo dt { font: 36px/40px ''; margin-bottom: 40px; }
.swipe { max-width: 1245px; margin: 120px auto 0; position: relative; }
#slider { overflow: hidden; visibility: hidden; position: relativel; margin: 0 100px; }
.swipe-wrap { overflow: hidden; position: relative; }
.swipe-wrap ul { float: left; width: 100%; position: relative; }
.swipe li { float: left; width: 50%; position: relative; overflow: hidden; }
.swipe li:hover .pic-txt { bottom: 0; }
.swipe img { display: block; width: 96%; margin: 0 auto; }
.swipe .pic-txt { width: 96%; margin-left: 2%; height: 100px; position: absolute; left: 0; bottom: -56px; transition: all .3s; background: #fff; background: rgba(0,0,0,.6); color: #fff; }
.swipe .pic-txt .p1 { font-size: 17px; height: 25px; line-height: 25px; margin:10px 0 5px; overflow: hidden; padding: 0 30px; }
.swipe .pic-txt .p2 { font-size: 13px; height: 48px; line-height: 24px; overflow: hidden; padding: 0 30px; position: relative; text-align: left; }
.swipe .prev, .swipe .next { width: 29px; height: 55px; background: url(/public/static/pc/images/arrow1.png) no-repeat; position: absolute; top: 50%; margin-top: -28px; cursor: pointer;-moz-user-select:none;user-select:none;-webkit-user-select:none;}
.swipe .prev { left: 50px; }
.swipe .next { right: 50px; background-position: 0 -60px; }
.bottomImg{ margin-top:140px; overflow:hidden;}
.bottomImg li{ float:left; width:25%; position:relative; overflow:hidden; background:#000;}
.bottomImg .pic-txt { width:100%;height:95px; position: absolute; left: 0; bottom:-53px; transition: all .3s;background: rgba(0,0,0,.6); color: #fff; text-align:center;}
.bottomImg li:hover .pic-txt{ bottom:0;}
.bottomImg .pic-txt .p1{ font-size: 17px; height: 25px; line-height: 25px; margin: 10px 0 5px; overflow: hidden; padding: 0 30px;}
.bottomImg .pic-txt .p2{ font-size: 13px; height: 48px; line-height: 24px; overflow: hidden; padding: 0 30px;}
.bottomImg img{ max-width:100%; display:block; opacity:0.6;}
.bottomImg li:hover img{ opacity:1;}

@media (max-width:1024px) {
.bottomImg li{width:50%;}
}
