@charset "utf-8";
/* CSS Document */
body { padding:0; margin:0; font-size:14px; background:#fafafa; color:#010101; font-family:"微软雅黑", "Arial"; min-width:320px;}
h1,h2,h3,h4,h5,h6,p,form,div,dl,dd,dt,ul,li,img { padding:0; margin:0; list-style-type:none; border:0; font-weight:normal;}
input,textarea,select,button { outline:none; list-style-type:none; border:none; padding:0; margin:0;}

a { text-decoration:none; color:#010101; outline:none;}
a:hover { text-decoration:none; color:#010101;}
body a{outline:none;blr:expression(this.onFocus=this.blur());}
.clearfix:after {content:"."; height:0px; line-height:0px; overflow:hidden; clear:both; display:block; visibility:hidden;}
.clearfix{zoom:1;}
.clear { margin:0;padding:0; border: none; clear:both; width:100%;}
input[type="button"], input[type="submit"], input[type="reset"], input[type="text"], input[type="password"] {-webkit-appearance: none; border-radius: 0;}

.main {margin: 0 auto; max-width: 640px; min-width: 320px; width: 100%; overflow: hidden;}
.w {margin: 0 auto; max-width: 640px; min-width: 320px; width: 100%; }
/*============================gonggong============================*/
.jj_box {display:box;display:-webkit-box;display:-moz-box; orient: horizontal; width:100%;}
.jj_1 { box-flex:1;-webkit-box-flex:1;-moz-box-flex:1; display:block;}


/*index/S*/
.header {height: 34px; margin-bottom: 20px;}
.header a {display: block; width: 20px; height: 20px; background: url(../images/diy_icon.png) left top no-repeat; margin: 12px 15px 0 15px;}
.header .fh {background-position: 0px -100px; float: left;  background-size: 20px 140px;}
.header .fx {background-position: 0px -130px; float: right;  background-size: 20px 150px;}

.diy_datu {height: 280px; text-align: center;}
.diy_datu img {height: 100%;}
.gzgnhx {background: #fff; box-shadow: 0 -5px 10px #cfcfcf; height:470px; margin-top:15px}
.diy_nav {width: 100%;}
.diy_nav ul {height: 48px; overflow: auto; width: 999px; float: left;}
.diy_nav ul li {width: 20%; text-align: center; height: 48px; overflow: hidden; float: left; cursor: pointer;}
.diy_nav ul li img {margin:0 auto; display:block; width:25px; height:auto;}
.diy_nav ul .active {color: #e66800; border-bottom: 2px solid #e66800;}
.diy_nav ul li p {width: 25px; height: 25px; background: url(../images/diy_icon.png) left 5px no-repeat; background-size: 24px 171px; margin: 3px auto auto auto;}
.diy_nav ul li .p2 {background-position: 4px -22px;}
.diy_nav ul li .p3 {background-position: 4px -48px;}
.diy_nav ul li .p4 {background-position: 2px -99px;}
.diy_nav ul li .p5 {background-position: 0px -73px;}


.wlkd {width: 100%; height: 200px; overflow: auto;}
.dzxx {padding:15px; float: left; width: 103%; box-sizing: border-box;}
.dzxx dd {float: left; width: 30%; margin: 0 3% 3% 0; text-align: center; position: relative;}
.dzxx dd.on:after {position: absolute; left: 50%; top: 50%; width: 24px; height: 24px; background: url(../images/xz.png) left top no-repeat; background-size: 24px 24px; content: ''; margin: -24px 0 0 -12px;}
.dzxx dd.on .p2 {color: #e66800;}
.dzxx dd .p1 {width: 100%; border: 1px solid #eee;}
.dzxx dd .p1 img {width: 100%;}
.dzxx dd .p2 {line-height: 24px; color: #666;}

.packing {padding:15px 15px 0 15px; width: 104%; box-sizing: border-box;}
.packing dd {float: left; width: 30%; height: 40px; line-height: 40px; border-radius: 5px; border: 1px solid #e8e8e8; margin: 0 3% 3% 0; text-align: center; box-sizing: border-box; color: #666;}
.packing .on {color: #e66800; line-height: 36px; border: 2px solid #e66800;}

.gxhbqsj {font-size: 14px; color: #222; height: 24px; line-height: 24px; margin: 5px 15px 0 15px;}
.design {width: 100%; overflow: hidden; padding: 15px; box-sizing: border-box;}
.design dd {float: left; width: 46%; text-align: center; margin-right: 2%; position: relative; border: 1px solid #dfdfdf; border-radius: 15px}
.design dd img {max-width: 100%; max-height: 100%; border-radius: 15px; display: block;}
.design .scsj {border:1px dashed #dfdfdf;}
.design .scsj input {position: absolute; left: 0; top: 0; width: 100%; height: 100%; opacity: 0;}

.gwc_box {border-top: 1px solid #dfdfdf; background-color: rgba(255,255,255,0.6); height: 40px; position: fixed; bottom: 0; left: 0; width: 100%; z-index: 9999;}
.gwc_box p {float: left; height: 40px; line-height: 40px; overflow:hidden; color: #686868; padding-left: 15px;}
.gwc_box p span {color: #e66800; font-size: 20px;}
.gwc_box p font {font-size: 14px;}
.gwc_box input,.gwc_box button {width: 120px; height: 40px; background: #e66800; color: #fff; float: right; font-size: 14px;}

/*index/E*/

.file_box span {
    background: rgba(0, 0, 0, 0) url("/diy/images/sctu.png") no-repeat scroll left top;
    display: block;
    height: 82px;
    width: 128px;
}
