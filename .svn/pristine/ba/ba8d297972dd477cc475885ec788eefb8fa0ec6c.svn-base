.swipe-wrap div {float: left;width: 100%;position: relative;}
.swipe-btn{ display:none; width:100%; text-align:center; position:absolute; left:0; bottom:20px;}
.swipe-btn li{ display:inline-block; margin:0 3px; width:15px; height:15px; overflow:hidden; border-radius:50%; border:solid 1px #fff;}
.swipe-btn .cur{ background:#fff;}
