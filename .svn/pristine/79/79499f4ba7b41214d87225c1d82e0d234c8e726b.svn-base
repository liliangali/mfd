/*购物车浮动层样式*/

.fixd_cart{width: 46px;position: fixed;bottom:112px;right:6px;z-index: 9;}
.fixd_cart a {width:46px; height:46px; line-height:42px; background:rgba(230,104,0,0.7); display:block; border-radius:50%; margin-bottom:6px; text-align:center; color:#fff;}
.fixd_cart a i {background:url("../../diy/images/diy_icon.png") no-repeat; background-size:46px; width:46px; height:46px; display:block;}
.fixd_cart a i.kefu_icon {background-position:0 -472px;}
.fixd_cart a i.cart_icon {background-position:0 -518px;}
.fixd_cart a i.ss_icon {background-position:0 -604px;}

.fixd_cart a:active {background:rgba(230,104,0,1);}


/*客服样式*/
#zhichiBtnBox {width:46px !important; height:46px !important; box-shadow:none !important; right:6px !important; bottom:64px !important; background-color:rgba(230,104,0,0.7) !important; background-image:url("../../diy/images/diy_icon.png") !important; background-repeat:no-repeat !important; background-size:46px !important; background-position:0 -472px !important;}
#zhichiBtnBox i {background:none !important; top:11px !important; left:7px !important; width:30px !important;}

