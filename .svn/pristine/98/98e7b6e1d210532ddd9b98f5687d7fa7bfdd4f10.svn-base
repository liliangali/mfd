body { margin: 0; padding: 0; background:#fff; color:#000; font:14px/1.5 arial,"Microsoft Yahei","Hiragino Sans GB",sans-serif; min-width:768px;}
div, form, ul, ol, li, span, p, dl, dt, dd, img, i, em, input, textarea, h1, h2, h3, h4, h5, h6 { margin: 0; padding: 0; border:0; outline: 0;}
ul, ol, li { list-style: none }
em, i { font-style: normal; }
body a{outline:none;blr:expression(this.onFocus=this.blur());}
a { color: #000; text-decoration: none; outline: 0; transition: color 0.3s ease 0s; }
a:hover { color: #5fc7ae; text-decoration:none; }
.clearfix:after {content:"."; height:0px; line-height:0px; overflow:hidden; clear:both; display:block; visibility:hidden;}
.clearfix{zoom:1;}
.area{overflow:hidden;}
.left,.fl{float:left;}
.right,.fr{float:right;}

/*head*/
.head_box {width:100%;  border-bottom:1px solid #ddd; background:#fff;}
.head { max-width:1230px; width:100%; padding:0 20px; margin:0 auto;height:100px; overflow:hidden;}
.head .logo{width:193px; height:39px; margin-top:35px;}
.head .logo img {width:100%;}
.head .navBox{margin-top:52px;}
.head li{float:left; padding-right:26px; font-size:20px; line-height:22px;}
.head .car {width:20px; height:22px; position:relative; position:relative;}
.head .car span { position:absolute; right:-9px; top:-15px; width:18px; height:18px; line-height:18px; background:#346fd6; border-radius:50%; text-align:center; color:#fff; font-size:12px;}
.head .car a{display:block; width:20px; height:18px; background:url(../../static/img/hd_icon01.png) no-repeat;  text-indent:-2000px;}
.head .uCenter a{ display:block; width:20px; height:18px; background:url(../../static/img/hd_icon02.png) no-repeat; margin-left:13px; text-indent:-2000px;}

/*footer*/
.footer{padding:65px 0; background:#2f3035;color:#a0a0a0; font-size:14px; }
.footer .w{ max-width:960px; min-width:768px; margin:0 auto; overflow:hidden;}
.footer dt{ font-size:18px; color:#fff;}
.footer dd{ margin-top:20px;}
.footer .box1{ width:390px;}
.footer .box1 a{ width:6em; margin-right:30px; float:left;color:#a0a0a0; text-decoration:none; margin-bottom:10px;}
.footer a:hover{ color:#fff;}
.footer .box2{ width:180px; padding:0 90px; font-size:13px; line-height:25px; border-left:solid 1px #37383d}
.footer .box2 p{ margin-top:10px;}
.footer .box3{ font-size:12px;border-left:solid 1px #37383d; padding-left:90px;}
.footer .box3 img{ margin-bottom:5px;}

@media (max-width:960px){
.footer .box1{ width:345px;}
.footer .box2{ padding:0 42px;}
.footer .box3{ padding-left:42px;}
}
