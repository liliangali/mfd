.lzsj h1 {font-weight:normal; font-size:35px; line-height:42px; font-family:宋体;}
.lzsj h2 {font-weight:normal; font-size:22px; line-height:42px;  font-family:宋体;}
.lzsj .p1 {font-size:14px; line-height:24px; margin-top:12px; width:80%; margin:0 auto;}
.lzsj .p2 {width:88px; height:93px; margin:0 auto; padding:25px 0 14px 0;}
.lzsj .p2 img {width:100%; border:1px solid #fff;}
