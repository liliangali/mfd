@charset "utf-8";
/* CSS Document */
.jj_box {display:box;display:-webkit-box;display:-moz-box; orient: horizontal; width:100%;}
.jj_1 { box-flex:1;-webkit-box-flex:1;-moz-box-flex:1; display:block;}
input [typut="text"]{-webkit-appearance: none;}

/*登录/S*/
.login { width:93%; margin:30px auto;}
.login p input { width:100%; height:48px; border:1px solid #ddd; padding:0 5px 0 10px;  color:#aaa; font-size:14px;}
.login p .input_1 {background:#fff  20px 12px no-repeat; background-size:20px 278px;}
.login p .input_2 { border-top:none; background:#fff url(/public/static/wapls/images/slx_icon.png) 20px -31px no-repeat; background-size:20px 278px;}
.ljzc {margin-bottom:200px;}
.ljzc a { display:block; background:#3f3b3a; color:#fff; height:40px; line-height:40px; text-align:center; font-size:16px; margin-top:24px;}

/*底部*/
.footer_box {width:100%; position:fixed; left:0; bottom:0;}
.footer { max-width:640px; margin:0 auto; height:48px; padding-top:10px; overflow:hidden;}
.yinyin { box-shadow:0 0 10px #eee;  background:#fefefe; width:100%; height:48px;}
.footer p { padding:0 30px; line-height:48px; height:48px; overflow:hidden;}
.footer .ljdl { text-align:center; color:#999}
.footer .ljdl a { color:#000; padding-left:5px;}
/*登录/E*/

/*会员注册/S*/
.sybox {width:93%; margin:0 auto;}



/*头/S*/
.header {width:100%; height:45px; position:relative; line-height:45px;}
.header .p1 { position:absolute; left:3.5%; top:0px;}
.header .p1 img { width:40%;}
.header .p2 { font-size:15px; text-align:left; color:#222; padding-left:43px;}
/*头/E*/

.hyzc { border:1px solid #d5d5d5; background:#fff; margin-top:30px;}
.hyzc p {border-bottom:1px solid #d5d5d5;}
.hyzc p:last-of-type {border-bottom:none;}
.hyzc input {border:none; width:100%; height:48px; box-sizing:border-box; background:#fff; padding:0 10px;}
.zzhq .getCode {display:block; line-height:30px; background:#eee; width:110px; height:30px; text-align:center; margin-top:9px; font-size:12px; color:#000; float:right; margin-right:10px;}
.zzhq .getCode.disabled{ color:#999;}
.zcxy_off { width:150px; margin:16px auto auto auto; color:#717171;  height:24px; line-height:24px; overflow:hidden; background:url(/public/static/wapls/images/slx_icon.png) left -103px no-repeat; background-size:19px 264px; padding-left:30px;}
.zcxy_on { width:150px; margin:16px auto auto auto; color:#717171;  height:24px; line-height:24px; overflow:hidden; background:url(/public/static/wapls/images/slx_icon.png) left -79px no-repeat; background-size:19px 264px; padding-left:30px;}
.zcxy_off a, .zcxy_on a { color:#000;}
