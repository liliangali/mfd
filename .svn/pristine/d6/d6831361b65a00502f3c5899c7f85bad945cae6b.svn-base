/* 消息列表 */
.wdxxlb { margin:0 10px; padding:10px 0 100px 0;}
.wdxxlb ul li {padding-right:40px; margin:0 10px; border-bottom:1px solid #ddd; padding:10px 5px; -moz-user-select:none;}
.wdxxlb ul li:last-of-type{ border:none;} 
.wdxxlb ul li h1 {font-size:14px; font-weight:normal; color:#222; height:20px; line-height:20px; position:relative;}
.wdxxlb ul li h1 b { display:block; width:8px; height:8px; background:#ff5500; position:absolute; left:-15px; top:7px; border-radius:50%;}
.wdxxlb ul li h1 .yxz {width:20px; height:20px; overflow:hidden; background:url(/public/static/wapls/images/slx_icon1.png) left -106px no-repeat; background-size:30px 250px; position:absolute; right:0; top:0; visibility:hidden; opacity:0; transform:translate3d(20px,0,0);-webkit-transform:translate3d(20px,0,0); transition:all .2s;-webkit-transition:all .2s;}
.wdxxlb ul.cur li h1 .yxz{visibility:visible; opacity:1; transform:translate3d(0,0,0);-webkit-transform:translate3d(0,0,0);}

.wdxxlb ul li.cur h1 .yxz { background-position:0 -73px;}
.wdxxlb ul li p {font-size:12px; color:#717171; line-height:20px; height:20px; overflow:hidden;}

.scfd { position:fixed; bottom:0; background:#fafafa; border-top:1px solid #f5f5f5; max-width:640px; min-width:320px; width:100%; margin:0 auto;transform:translate3d(0,45px,0);-webkit-transform:translate3d(0,45px,0); transition:all .2s;-webkit-transition:all .2s;}
.scfd.cur{transform:translate3d(0,0,0);-webkit-transform:translate3d(0,0,0);}
.scfd ul li { float:left; width:50%; text-align:center; height:44px; line-height:44px; font-size:14px;}
