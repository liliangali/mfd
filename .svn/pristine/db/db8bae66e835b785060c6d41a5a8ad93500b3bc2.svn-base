*{margin:0;padding:0;}
body{font-family: "微软雅黑", "Arial";background:#fff;min-width:768px;color:#000;}
h1, h2, h3, h4, h5, h6, p, form, div, dl, dd, dt, ul, li, img{list-style-type: none;border:0;font-weight:normal;}
.left{float:left;}
.right{float:right;}
img{display:block;}
a{text-decoration:none;transition:color 0.6s ease 0s;}
a,a:visited{color:#3d3d3d;text-decoration:none;transition:color 0.6s ease 0s;}
a:hover{text-decoration:none;color:#63C;}
/*布局*/
.container{margin:0 auto;max-width:1539px;}
/*头部*/
.head{height:143px;padding:0 70px;}
.head .logo{width:193px; height:39px;margin-top:56px;}
.head .navBox{margin-top:75px;}
.head li{float:left; padding-left:27px;}
.head .car a{display:block; width:20px; height:18px; background:url(../../static/img/hd_icon01.png) no-repeat; margin-left:13px; text-indent:-2000px;}
.head .uCenter a{ display:block; width:20px; height:18px; background:url(../../static/img/hd_icon02.png) no-repeat; margin-left:13px; text-indent:-2000px;}
/*banner*/
.banner{width:100%;height:100%;margin:0 auto;}
.banner img{width:100%;height:100%;}
/*内容*/
.content{max-width:1290px;margin:0 auto;overflow:hidden;}
.nrleft{overflow:hidden;color:#3d3d3d;width:64.6%;border-right:1px solid #e5e5e5;padding-right:7%;padding-bottom:107px;margin-top:115px;}
.nrleft img{width:100%;height:100%;}
.nrleft h2{font-size:34px;}
.nrleft .tada{height:73px;line-height:73px;color:#8f8f8f;}
.nrleft .wordtest{font-size:14px;line-height:24px;margin-top:24px;}
.nrleft .next{ width:167px;height:50px; line-height:50px; border:1px solid #9d9d9d; text-align:center;font-size:19px; margin-top:49px;}
.nrleft .next a{ display:block;}
.nrleft .supplement{overflow:hidden;}
.nrleft .supplement h4{line-height:89px;font-size:18px;}
.nrleft .supplement .wordtests{font-size:14px;line-height:24px;}
.nrright{overflow:hidden;width:24.9%;margin-top:105px;}
.nrright h4{font-size:20px;}
.nrright .nrright_ul{margin-top:36px;}
.nrright .nrright_ul li{height:40px;line-height:40px;overflow:hidden;}
.years{font-size:14px;line-height:14px;margin-top:82px;color:#101010;}
/*日历*/
#wrap{width:320px;border:solid 1px #ccc;margin:0 auto;margin-top:26px;}
#kalendar{width:320px;margin:0 auto;}
/* 选择年月 的行 */
#kalendar tr#select{height:30px;background:#eee;}
#kalendar tr#select td{clear:both;}
#kalendar tr#select td div{float:left;width:160px;height:30px;line-height:30px;}
#kalendar tr#select #year,#kalendar tr#select #month{width:135px;padding-left:20px;}
#select #year ul,#select #month ul{clear:both;}
#select #year ul li,#select #month ul li{float:left;width:30px;}
#select #year ul li input,#select #month ul li input{width:30px;text-align:30px;cursor:pointer;}
#select #year ul li.selectChange,#select #month ul li.selectChange{width:70px;height:10px;}
#select #year ul li.selectChange select,#select #month ul li.selectChange select{width:70px;padding-left:2px;cursor:pointer;}
#select #year ul li.selectChange select option,#select #month ul li.selectChange select option{padding-left:5px;}
/* 星期表头 的行 */
#kalendar tr#week{height:50px;}
#kalendar tr#week td ul{width:320px;clear:both;}
#kalendar tr#week td ul li{float:left;width:43px;height:49px;line-height:50px;text-align:center;background:#9999CC;margin:0 1px;}

/* 某月具体日期 的每一行 */
#kalendar tr#day ul.dayList{width:320px;height:50px;clear:both;}
#kalendar tr#day ul.dayList li{float:left;width:43px;height:49px;line-height:50px;text-align:center;cursor:pointer;margin:0 1px;background:#eee;}
/* 日期占用行数(4-6行)，将不需要显示的行隐藏 */
#kalendar tr#day ul.hide{display:none;}
/* 周末 的特殊样式 */
#week .weekend,#day .weekend{color:#EA0000;}

/* 下面几个样式注意顺序，选择器不能简写，否则日历中 今天 的样式会被鼠标滑过的样式替换 */
/* 鼠标经过某个日期 的特殊样式 */
#kalendar tr#day td ul.dayList li.mouseFloat{background:#FFDCB9;}
/* 当天是周末 的特殊样式 */
#kalendar tr#day td ul.dayList li.todayWeekend{color:#EA0000; background:#FFDCB9;font-weight:700;}
/* 当天 的特殊样式 */
#kalendar tr#day td ul.dayList li.today{background:#abcdef;font-weight:700;}
/*底部*/
.footer{overflow:hidden;padding:65px 543px; background:#2f3035;margin-top:150px;color:#cbcbcc; font-size:14px; text-align:center; min-width:768px;}
.footer .contact{padding-top:21px;}
.footer .time{padding-left:9%;padding-top:21px;}
.footer .scanning{ width:110px;text-align:center;padding-left:26%;font-size:12px;}
.footer .scanning img{width:110px;height:110px;overflow:hidden;}
@media (min-width: 768px) and (max-width:980px){
.container {width:768px; }
.footer{padding:65px 73px;}
.footer .time{padding-left:7%;}
.footer .scanning{padding-left:12%;}
}
@media (min-width: 980px) and (max-width:1024px){
.container {width:980px; }
.footer{padding:65px 100px;}
.footer .time{padding-left:11%;}
.footer .scanning{padding-left:25%;}
}
@media (min-width: 1024px) and (max-width:1280px) {
.container {width:1024px;}
.content dl{overflow:hidden;padding-right:3%;}
.footer{padding:65px 153px;}
.footer .time{padding-left:7%;}
.footer .scanning{padding-left:21%;}
}
@media (min-width: 1280px) and (max-width:1440px) {
.container {width:1280px;}
.footer{padding:65px 232px;}
.footer .time{padding-left:10%;}
.footer .scanning{padding-left:26%;}
}
@media (min-width: 1440px) and (max-width:1540px) {
.container {width:1440px;}
.footer{padding:65px 310px;}
.footer .time{padding-left:10%;}
.footer .scanning{padding-left:26%;}
}
@media (min-width: 1540px) {
.container {width: 1540px; }
}

