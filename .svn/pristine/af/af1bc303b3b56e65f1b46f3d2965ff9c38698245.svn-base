.win .woman{ width:215px;}
.win .tit{ height:25px;font-size:14px;line-height:25px;}
.win .man .small{ width:177px; height:148px;}
.win .man .big{ width:190px; height:304px;}
.win .recomm .small{ width:145px; height:148px;}
.win .woman .big{ width:215px; height:304px;}
.win .mt{ margin-top:10px;}
