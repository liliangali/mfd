.win .recomm .small{ width:189px; height:193px;}
.win .woman .big{ width:280px; height:396px;}
.win .mt{ margin-top:10px;}
.appShow{ width:990px; margin:0 auto;}
}
@media screen and (max-width:990px) {
.win{ width:760px; margin:50px auto 0;}
