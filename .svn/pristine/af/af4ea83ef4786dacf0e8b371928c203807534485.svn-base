====================*/
.priceBox { text-align: center; color: #ff5500; font-size: 25px; }
.btn { text-align: center; margin-top: 10px; }
.btn span { width: 50%; max-width: 200px; height: 35px; line-height: 35px; background: #21d0fd; color: #fff; font-size: 14px; display: inline-block; border-radius: 35px; }
.btn span:active, .btn span:hover { opacity: 0.9 }
.btn em { display: inline-block; width: 10%; font-style: normal; color: #666; }
.opt { overflow: hidden; width: 100%; max-width: 640px; position:absolute; bottom: 0; transform: translateY(220px); -webkit-transform: translateY(220px); transition: all .5s; -webkit-transition: all .5s; }
.opt .minTit { padding: 5px 1.5%; clear: both; color: #666; }
.opt .scrollBox { height: 175px; overflow-x: hidden; overflow-y: auto; }
.opt.show { transform: translateY(0); -webkit-transform: translateY(0) }
#switch { text-align: center; animation: scroll-arrow-animation 750ms ease-in-out infinite alternate; -moz-animation: scroll-arrow-animation 750ms ease-in-out infinite alternate; -webkit-animation: scroll-arrow-animation 750ms ease-in-out infinite alternate; }
@keyframes scroll-arrow-animation { from {
transform:translateY(0);
-webkit-transform:translateY(0)
}
to { transform: translateY(10px); -webkit-transform: translateY(10px) }
}
@-webkit-keyframes scroll-arrow-animation { from {
transform:translateY(0);
-webkit-transform:translateY(0)
}
to { transform: translateY(10px); -webkit-transform: translateY(10px) }
}
.opt .angle { display: inline-block; width: 72px; height: 30px; background: url(../images/angleup.png) no-repeat; margin-bottom: 10px; }
.opt.show .angle { background: url(../images/angledown.png) no-repeat; }
.opt .catBox { height: 220px; background: rgba(255,255,255,0.9); box-shadow: 0 0 20px #a8adb2; }
.opt .cat1 { text-align: center; overflow: hidden; }
.opt .cat1 li { width: 25%; float: left; margin-top: 20px; color: #666; font-size: 12px; }
#nav-fabric:before { content: ''; display: block; margin: 0 auto; background: url(../images/catico.png) no-repeat; background-size: 273px auto; width: 66px; height: 66px; }
#nav-lining:before { content: ''; display: block; margin: 0 auto; background: url(../images/catico.png) no-repeat -69px 0; background-size: 273px auto; width: 66px; height: 66px; }
#nav-button:before { content: ''; display: block; margin: 0 auto; background: url(../images/catico.png) no-repeat 0 -69px; background-size: 273px auto; width: 66px; height: 66px; }
#nav-style:before { content: ''; display: block; margin: 0 auto; background: url(../images/catico.png) no-repeat -138px 0; background-size: 273px auto; width: 66px; height: 66px; }
#nav-embroidery:before { content: ''; display: block; margin: 0 auto; background: url(../images/catico.png) no-repeat -207px 0; background-size: 273px auto; width: 66px; height: 66px; }
#nav-keyhole:before { content: ''; display: block; margin: 0 auto; background: url(../images/catico.png) no-repeat -69px -69px; background-size: 273px auto; width: 66px; height: 66px; }
#nav-studs:before { content: ''; display: block; margin: 0 auto; background: url(../images/catico.png) no-repeat -138px -69px; background-size: 273px auto; width: 66px; height: 66px; }
#nav-process{ display:none;/*暂时屏蔽工艺*/}
/*#nav-process:before { content: ''; display: block; margin: 0 auto; background: url(../images/catico.png) no-repeat -207px -69px; background-size: 273px auto; width: 66px; height: 66px; }*/
.opt .cat2 { display: none; }
.opt .cat2 .loading { text-align: center; font-size: 14px; color: #5ebfff; line-height: 100px; }
.opt .cat2 .titBar { height: 45px; line-height: 45px; padding-right: 15px; overflow: hidden; }
.opt .cat2 .titBar .tit { background: url(../images/minback.png) no-repeat 13px center; background-size: auto 15px; padding: 0 30px; }
.opt .cat2 .titBar .del { margin-left: 10px; }
.opt .cat2 .itemList { margin: 2% 8px 0; overflow: hidden; padding-bottom: 20px; }
.opt .cat2 .itemList .status { text-align: center; line-height: 70px; color: #f00; }
.opt .cat2 .itemList li { width: 25%; float: left; position: relative; text-align: center; font-size: 10px; margin-bottom:35px;}
.opt .cat2 .itemList li p { height: 25px;line-height: 13px; position: absolute; bottom: -30px; left:10%; width:80%; color: #333; white-space:nowrap; overflow:hidden; text-overflow:ellipsis;}
.opt .cat2 .itemList li .ico-first { width: 50%; height: 50%; background: url(../images/first.png) no-repeat; background-size: contain; position: absolute; top: 0; right: 0; }
.opt .cat2 .itemList li .ico-hd { width: 50%; height: 50%; background: url(../images/hd.png) no-repeat; background-size: contain; position: absolute; top: 0; right: 0; }
.opt .cat2 .itemList li img { margin: 0 auto; width: 90%; height: auto; box-sizing: border-box; border: solid 1px #dfe4e9; border-radius: 5px; background: #fff; }
.opt .cat2 .itemList li.cur:after { content: ''; display: block; width: 36px; height: 36px; position: absolute; left: 50%; top: 50%; margin: -18px 0 0 -18px; background: url(../images/dui.png) no-repeat center center #21d0fd; background-size: 60% auto; border-radius: 50%; overflow: hidden; }
.opt .cat2 .itemList li .tit { position: absolute; left: 0; bottom: 10px; width: 100%; text-align: center; font-size: 12px; }
.opt .cat2 .itemList .null { height: 70px; line-height: 60px; text-align: center; font-size: 12px; }
/*面料*/
#opt-fabric .subCat { margin: 0 8px; }
#opt-fabric .cat2-1, #opt-fabric .cat2-2 { margin: 0 1.5%; }
#opt-fabric .cat2-1 ul { width: 100%; text-align: center; font-size: 14px; margin-top: 1px; display: box; display: -webkit-box; display: -moz-box; orient: horizontal; }
#opt-fabric .cat2-1 li { background: none repeat scroll 0 0 #fff; border: 1px solid #dfe4e9; padding: 10px 0; box-flex: 1; -webkit-box-flex: 1; -moz-box-flex: 1; }
#opt-fabric .cat2-1 li:first-child { border-radius: 5px 0 0; border-right: 0; }
#opt-fabric .cat2-1 li:last-child { border-radius: 0 5px 0 0; border-left: 0; }
#opt-fabric .cat2-1 li:nth-child(2) { border-right: 0; }
#opt-fabric .cat2-1 li.cur { background: #21d0fd; color: #fff; }
#opt-fabric .cat2-2 { font-size: 14px; color: #666; min-height:5px; background:#fff;text-align: center; border: solid 1px #dfe4e9; border-top: 0; overflow: hidden; border-radius: 0 0 5px 5px; }
#opt-fabric .cat2-2 span { width: 20%; box-sizing: border-box; display: inline-block; white-space: nowrap; padding: 11px 0 10px; float: left; overflow:hidden; text-overflow:ellipsis;}
#opt-fabric .cat2-2 .cur { color: #21d0fd }
#opt-fabric .itemList li { width: 33.33%;}
/*款式*/
#opt-style .cat2-1 { margin: 0 8px; font-size: 14px; }
#opt-style .cat2-1 ul { margin-top: 1px; text-align: center; }
#opt-style .cat2-1 li { display: inline-block; width: 32%; padding: 10px 0; margin: -1px 0 0 -1px; border: solid 1px #dfe4e9; background: #fff; }
#opt-style .cat2-1 li:first-child { border-radius: 5px 0 0 }
#opt-style .cat2-1 li:last-child { border-radius: 0 0 5px 0 }
#opt-style .cat2-1 li:nth-child(3) { border-radius: 0 5px 0 0 }
#opt-style .cat2-1 li:nth-last-child(3) { border-radius: 0 0 0 5px; }
#opt-style .cat2-1 .cur { background: #21d0fd; color: #fff; }
#opt-style .weitiao { color: #fff; line-height: 30px;}
#opt-style .weitiao span { background: #cee1ee;text-align: center;  border-radius: 5px; display: inline-block; margin: 1.5%; width: 22%; }
#opt-style .weitiao .cur { background: #21d0fd; }
/*刺绣*/
.opt .cxCon { margin: 5px 1.5%; }
.opt .cxCon input { width: 100%; height: 40px; padding: 3px; border-radius: 3px; box-sizing: border-box; border: solid 1px #ddd; }
.opt .cxPosition { padding: 0 1.5%; font-size: 14px; }
.opt .cxPosition p {overflow: hidden; padding: 5px 0 5px 30px; color: #666; }
.opt .cxPosition span { width: 20px; height: 20px; position: relative;border-radius: 50%; float:left; margin-left:-26px; box-sizing: border-box; border: solid 1px #c7d6df; }
.opt .cxPosition .cur span { background: #21d0fd; border: 0; }
.opt .cxPosition .cur span:after { content: ''; display: block; width: 90%; height: 90%; position: absolute; left: 5%; top: 5%; background: url(../images/dui.png) no-repeat center center; background-size: 60% auto; overflow: hidden; }
/*锁眼*/
#opt-keyhole .cat2-1 { padding: 0 8px; font-size: 14px; }
#opt-keyhole .cat2-1 ul { margin-top: 1px; text-align: center; }
#opt-keyhole .cat2-1 li { display: inline-block; width: 32%; padding: 10px 0; margin: -1px 0 0 -1px; border: solid 1px #dfe4e9; background: #fff; }
#opt-keyhole .cat2-1 .current { background: #21d0fd; color: #fff; }
#opt-keyhole .cat2-1 li:first-child { border-radius: 5px 0 0 }
#opt-keyhole .cat2-1 li:last-child { border-radius: 0 0 5px 0 }
#opt-keyhole .cat2-1 li:nth-child(3) { border-radius: 0 5px 0 0 }
#opt-keyhole .cat2-1 li:nth-last-child(3) { border-radius: 0 0 0 5px; }
/*工艺*/
#opt-process .itemList li img { margin-bottom: 30px; }
/*尺码*/
.sizeTable { bottom: 0; max-width: 640px; overflow: hidden; position: absolute; z-index: 102; width: 100%; background: rgba(255, 255, 255, 0.9); height: 300px; transform: translateY(300px); -webkit-transform: translateY(300px); transition: all .5s; -webkit-transition: all .5s; }
.sizeTable .step1 { line-height: 120px; text-align: center; }
.sizeTable .step1 li { display: inline-block; width: 120px; height: 120px; border: solid 2px #ddd; margin: 50px 10px; border-radius: 20px; background-color: #fafafa; background-image: linear-gradient(#fafafa, #e3e3e3); background-image: -webkit-linear-gradient(#fafafa, #e3e3e3); background-image: -moz-linear-gradient(#fafafa, #e3e3e3); }
.sizeTable .step1 li:hover, .sizeTable .step1 li:active { color: #666; background: #fff; border-color: #21d0fd; }
.sizeTable .step2 { display: none; }
.sizeTable .titBar { padding: 5px 1.5%; clear: both; color: #666; height: 45px; line-height: 45px; padding-right: 15px; border-bottom: solid 1px #d5d5d5; overflow: hidden; }
.sizeTable .titBar .tit { color: #000; background: url(../images/minback.png) no-repeat scroll 13px center; background-size: auto 15px; padding: 0 30px; }
.sizeTable .titBar .seeSize { color: #21d0fd; }
.sizeTable .scrollBox { clear: both; height: 193px; overflow-x: hidden; overflow-y: auto; }
.sizeTable.show { transform: translateY(0); -webkit-transform: translateY(0) }
.sizeTable .tabBar { padding: 10px; color: #666; overflow: hidden; }
.sizeTable .tabBar .tab { line-height: 1.5; }
.sizeTable .tabBar .tab li { float: left; border-bottom: solid 2px rgba(255,255,255,0); padding: 0 20px; }
.sizeTable .tabBar .tab .cur { color: #21d0fd; border-color: #21d0fd; }
.sizeTable .tabBar .save { background: #21d0fd; border-radius: 5px; color: #fff; padding: 2px 10px; }
.sizeTable .itemList { padding: 0 8px; overflow: hidden; line-height: 30px; color: #666; font-size: 14px; }
.sizeTable .itemList li { float: left; width: 22%; margin: 1.5%; text-align: center; background: #fff; border-radius: 5px; border: solid 1px #d5d5d5; box-sizing: border-box; }
.sizeTable .itemList .cur { border-color: #21d0fd; color: #21d0fd; }
.sizeTable .loading { text-align: center; line-height: 140px; }
