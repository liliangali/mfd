body, div, p, ul, ol, li, nav, footer, dl, dt, dd, h1, h2, h3, h4, h5, h6, form, img { padding: 0; margin: 0; }
body { min-width: 320px; margin: 0 auto; font: normal 18px/1.5 helvetica, verdana, san-serif; background: #ecf8fa; overflow: hidden; }
ul, ol, li { list-style: none; }
* {	-webkit-tap-highlight-color: rgba(255,255,255,0);}
img { vertical-align: middle; display: block; border: none; }
a { text-decoration: none; }
.fl { float: left; }
.fr { float: right; }
.zhezhaoLayer { position: fixed; top: 0; z-index: 101; width: 100%; max-width: 640px; height: 100%; background: #000; opacity: 0.2; }
.showLoading .zhezhaoLayer { background: url(../images/loading2.gif) no-repeat center center #000; }
.hide { display: none; }
.refresh { background: rgba(0, 0, 0, 0.7); border-radius: 5px; color: #fff; display: none; left: 50%; margin-left: -105px; padding: 10px; position: absolute; text-align: center; top: 45%; width: 190px; z-index: 102; }
.refresh a { color: #f50 }
/*模拟alert*/
#luck { position: absolute; left: 0; top: 0; width: 100%; height: 100%; z-index: 19891014; }
.luck-shade { background: #000; opacity: 0.5; position: fixed; left: 0; top: 0; width: 100%; height: 100%; z-index: 0; }
.luck-table { position: fixed; left: 0; top: 0; width: 100%; height: 100%; display: table; pointer-events: none; z-index: 1; }
.luck-table-cell { display: table-cell; vertical-align: middle; text-align: center; position: relative; z-index: 101; }
.luck-child { background: #fff; display: inline-block; max-width: 260px; min-width: 150px; border-radius: 5px; overflow: hidden; line-height: 22px; pointer-events: auto; animation: bouncedelay ease .2s; -webkit-animation: bouncedelay ease .2s; box-shadow: 0 0 50px #666; text-align: left; }
@-webkit-keyframes bouncedelay { 0% {
-webkit-transform:scale(0)
}
 100% {
-webkit-transform:scale(1)
}
}
@keyframes bouncedelay { 0% {
transform:scale(0);
-webkit-transform:scale(0)
}
 100% {
transform:scale(1);
-webkit-transform:scale(1)
}
}
.luck-con { padding: 20px 15px; font-size: 14px; }
.luck-title { height: 40px; line-height: 40px; background: #fff; font-weight: normal; padding-left: 10px; border-bottom: solid 1px #ddd; position: relative; }
.luck-title button { width: 40px; height: 40px; background: none; border: 0; font-size: 18px; position: absolute; right: 0; top: 0; }
/*颜色*/
.orange { color: #f6b46e; }
/*布局*/
.main { width: 100%; max-width: 640px; overflow: hidden; margin: 0 auto; position: relative; }
/*展示区*/
.diyShow { position: relative; padding: 20px 0; }
.tzBtnBox { font-size: 12px; height: 60px; text-align: center; }
.tzBtnBox span { margin: 0 6px; display: inline-block; box-sizing: border-box; border: solid 2px #9ee6f8; color: #95a1aa; width: 50px; line-height: 46px; text-align: center; height: 50px; border-radius: 50%; transition: all .2s; overflow:hidden;}
.tzBtnBox span.cur { transform: scale(1.1); -webkit-transform: scale(1.1); border-radius: 50%; color: #fff; border-color: #21d0fd; background: #21d0fd }
.showArea { height: 800px; position: relative; margin: 0 auto; max-width: 560px; z-index: 0; overflow: visible; text-align:center; }
.showArea canvas { position: absolute; max-width: 100%; max-height: 100%; left: 0; top: 0; }
.showArea img { max-width: 100%; max-height: 100%; display:inline; }
