}
/* Multiple select */
.android-ics.light .dwwms .dw-li:after {
	text-shadow: 0 0 5px #31b6e7;
}

/*动画库样式*/

.animated {
	-webkit-animation-duration:1s;
	animation-duration:1s;
	-webkit-animation-fill-mode:both;
	animation-fill-mode:both
}
.c_infinite {
	-webkit-animation-iteration-count:5;
	animation-iteration-count:5
}
.animated.hinge {
	-webkit-animation-duration:2s;
	animation-duration:2s
}

@-webkit-keyframes pulse {
	0% {
		-webkit-transform:scale3d(1,1,1);
		transform:scale3d(1,1,1)
	}
	50% {
		-webkit-transform:scale3d(1.05,1.05,1.05);
		transform:scale3d(1.05,1.05,1.05)
	}
	100% {
		-webkit-transform:scale3d(1,1,1);
		transform:scale3d(1,1,1)
	}
}@keyframes pulse {
	 0% {
		 -webkit-transform:scale3d(1,1,1);
		 -ms-transform:scale3d(1,1,1);
		 transform:scale3d(1,1,1)
	 }
	 50% {
		 -webkit-transform:scale3d(1.05,1.05,1.05);
		 -ms-transform:scale3d(1.05,1.05,1.05);
		 transform:scale3d(1.05,1.05,1.05)
	 }
	 100% {
		 -webkit-transform:scale3d(1,1,1);
		 -ms-transform:scale3d(1,1,1);
		 transform:scale3d(1,1,1)
	 }
 }
.pulse {
	-webkit-animation-name:pulse;
	animation-name:pulse
}


/*布局*/

.c_db {
	display:box;
	display:-webkit-box;
	display:-moz-box;
}

.c_bf1 {
	-moz-box-flex:1;
	-webkit-box-flex:1;
	box-flex:1;
}

.c_bf2 {
	-moz-box-flex:2;
	-webkit-box-flex:2;
	box-flex:2;
}

.c_bf3 {
	-moz-box-flex:3;
	-webkit-box-flex:3;
	box-flex:3;
}


/*box-align布局_垂直对齐方式*/
.c_bas {
	-moz-box-align:start;
	-webkit-box-align:start;
	-o-box-align:start;
	box-align:start;//顶对齐
}

.c_bac {
	-moz-box-align:center;
	-webkit-box-align:center;
	-o-box-align:center;
	box-align:center;//居中
}

.c_bae {
	-moz-box-align:end;
	-webkit-box-align:end;
	-o-box-align:end;
	box-align:end;//底对齐
}

.c_bast {
	-moz-box-align:stretch;
	-webkit-box-align:stretch;
	-o-box-align:stretch;
	box-align:stretch;//拉伸对齐
}


/*box-pack布局_水平对齐方式*/
.c_bps {
	-moz-box-pack:start;
	-webkit-box-pack:start;
	-o-box-pack:start;
	box-pack:start;//左对齐
}

.c_bpc {
	-moz-box-pack:center;
	-webkit-box-pack:center;
	-o-box-pack:center;
	box-pack:center;//居中对齐
}

.c_bpe {
	-moz-box-pack:end;
	-webkit-box-pack:end;
	-o-box-pack:end;
	box-pack:end;//右对齐
}

.c_bpj {
	-moz-box-pack:justify;
	-webkit-box-pack:justify;
	-o-box-pack:justify;
	box-pack:justify;//水平等分父容器宽度
}


/*box-orient布局_水平还是垂直*/
.c_bov {
	-moz-box-orient:vertical;
	-webkit-box-orient:vertical;
	box-orient:vertical;//垂直
}

.c_boh {
	-moz-box-orient:horizontal;
	-webkit-box-orient:horizontal;
	box-orient:horizontal;//水平
}


/*box-direction布局_排列顺序*/
.c_bdr {
	-moz-box-direction:reverse;
	-webkit-box-direction:reverse;
	box-direction:reverse;//设置为反转
}


.c_bs{
	-moz-box-sizing:border-box;
	-webkit-box-sizing:border-box;
	box-sizing:border-box;
}


