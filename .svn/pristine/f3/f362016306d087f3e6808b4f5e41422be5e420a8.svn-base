.android-ics.light .dw {
	background: #f5f5f5;
}
.android-ics.light .dww .dw-li {
	color: #000;
}
.android-ics.light .dwwo {
	background: linear-gradient(#f5f5f5 0%,rgba(245,245,245,0) 52%, rgba(245,245,245,0) 48%, #f5f5f5 100%);
