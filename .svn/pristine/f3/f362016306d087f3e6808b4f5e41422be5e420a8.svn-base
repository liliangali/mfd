	}
	to {
		opacity: 0;
		-moz-transform: scale(0.8);
	}
}
/* Flip in */
@keyframes dw-fl-in {
	from {
		opacity: 0;
		transform: rotateY(90deg);
	}
	to {
		opacity: 1;
		transform: rotateY(0);
	}
}
