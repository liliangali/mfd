	}
}
/* Flip out */
@keyframes dw-fl-out {
	from {
		opacity: 1;
		transform: rotateY(0deg);
	}
	to {
		opacity: 0;
		transform: rotateY(-90deg);
	}
}
@-webkit-keyframes dw-fl-out {
	from {
		opacity: 1;
