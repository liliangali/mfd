/*--模拟select/S--*/
.s_select { width: 120px; height: 28px; float: left; position: relative; z-index: 3; }
.s_select dt { width: 100%; height:28px; line-height: 28px; background: #fff; border: solid 1px #ccc; cursor: pointer; -moz-user-select: none; }
.s_select dt span { display: block; margin-right: 30px; padding-left: 10px; white-space: nowrap; overflow: hidden;}
.s_select dt i { position: absolute; right: 10px; top: 13px; width: 0; height: 0; line-height: 0; overflow: hidden; border-style: solid; border-color: #999 #fff #fff; border-width: 7px 5px 0; }
.s_select dd { width: 100%; max-height: 240px; overflow-x: hidden; overflow-y: auto; border: solid 1px #ddd; background: #fff; position: absolute; left: 0; top: 29px; display: none; }
.s_select dd a { display: block; height: 30px; line-height: 30px; padding: 0 10px; text-decoration: none; white-space: nowrap; overflow: hidden; text-overflow: ellipsis; }
.s_select dd a:hover { background: #f5f5f5; color: #000; }
.s_select_cur dt, .s_select_cur dd { border-color: #000; }
/*--模拟select/E--*/

