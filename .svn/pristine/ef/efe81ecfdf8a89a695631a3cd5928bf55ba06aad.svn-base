/*--基本表单/S--*/
.myForm { width:920px;margin:20px; padding-bottom:100px; }
.myForm .item {min-height:32px; line-height:32px; padding-left:90px; margin-bottom:15px; clear:both; }
.myForm .txt { width:230px; height:30px; line-height:30px\9; padding:0 3px;background: #fff; border: solid 1px #ddd; color:#222;}
.myForm .txt:focus { border-color: #000;}
.myForm .item .tit { width:90px; margin-left:-90px;line-height:30px; float:left;font-size: 14px;}
.myForm .item .tit em{ display:inline-block; width:11px; color:#a40001; font-family:Verdana, Geneva, sans-serif;}
.myForm .item label{margin:5px 20px 0 0; float:left; color:#222; font-size:14px; line-height:normal;}
.myForm .item .tip{width:480px; padding:5px;line-height:20px; color:#333;}
.myForm .sendValidate {width: 111px; height: 30px; line-height: 30px; text-align: center; z-index: 2; color: #333; border: solid 1px #ddd; border-radius: 2px; cursor: pointer; background-color: #f5f5f5; background-image: linear-gradient(#fff, #ebebeb); background-image: -webkit-gradient(#fff, #ebebeb); background-image: -moz-linear-gradient(#fff, #ebebeb); -moz-user-select: none; user-select: none; }
.myForm .sendValidate:hover { background-image: linear-gradient(#ebebeb, #fff); background-image: -webkit-gradient(#ebebeb, #fff); background-image: -moz-linear-gradient(#ebebeb, #fff); }
.myForm .sendValidate.disabled, .myForm .sendValidate.disabled:hover { background: #f8f8f8; cursor: default; }
.myForm label input{ float:left; margin:3px 4px 0 0;}
.myForm .depict{ width:480px; height:80px; border:solid 1px #ddd;line-height:24px; font-size:12px; color:#666; padding:0 3px;}
.myForm .depict:focus { border-color: #000; color:#333;}
.myForm .submit {display:block; width:230px; height:40px; line-height:40px; text-align:center; margin-left:91px; background:#346fd6; font-size:15px; color: #fff; cursor: pointer; border-radius:5px; transition: all 1s ease 0s;}
.myForm .submit span{ display:block; margin-left:3px;}
.myForm .submit:hover { background:#6b9aed; }
.myForm .submit:hover span { background-position: right -46px; }
.myForm .submit:active { opacity: 0.8; }
#s_2,#s_3{ margin-left:12px; float:left; position:relative; z-index:10;}
#ul_li input{ width:250px; padding:2px 10px; background:#ddd;}
/*--基本表单/E--*/

