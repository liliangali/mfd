@charset "utf-8";
/* CSS Document */

body { margin: 0; padding: 0; background:#fff; color:#000; font:14px/1.5 arial,"Microsoft Yahei","Hiragino Sans GB",sans-serif; min-width:768px;}
div, form, ul, ol, li, span, p, dl, dt, dd, img, i, em, input, textarea, h1, h2, h3, h4, h5, h6 { margin: 0; padding: 0; border:0; outline: 0;}
ul, ol, li { list-style: none }
em, i { font-style: normal; }
a { color: #000; text-decoration: none; outline: 0; transition: color 0.3s ease 0s; }
a:hover { color: #5fc7ae; text-decoration:none; }

.clearfix:after {content:"."; height:0px; line-height:0px; overflow:hidden; clear:both; display:block; visibility:hidden;}
.clearfix{zoom:1;}
.area{overflow:hidden;}

.left{float:left;}
.right{float:right;}
.container{margin:0 auto;max-width:1420px;}

/*头部*/
.head {height:118px; overflow:hidden; }
.head .logo{width:193px; height:39px; margin-top:56px;}
.head .logo img {width:100%;}
.head .navBox{margin-top:75px;}
.head li{float:left; padding-right:26px; font-size:16px; line-height:22px;}
.head .car a{display:block; width:20px; height:18px; background:url(../../static/img/hd_icon01.png) no-repeat;  text-indent:-2000px;}
.head .uCenter a{ display:block; width:20px; height:18px; background:url(../../static/img/hd_icon02.png) no-repeat; margin-left:13px; text-indent:-2000px;}

/*内容*/
.banner {width:100%;}
.banner img {width:100%;}

.yxbt {width:50%; margin:0 auto; text-align:center; padding:65px 0 40px 0;}
.yxbt h1 { font-size:58px; color:#373737; line-height:60px;}
.yxbt p {font-size:16px; line-height:24px; color:#656565; margin-top:25px;}

.cllb {width:1170px; margin:0 auto; overflow:hidden;}
.cllb ul { float:left; width:105.5%;}
.cllb ul li { float:left; width:30%; margin:0 30px 48px 0;}
.cllb ul li:hover .p2 { color:#5fc7ae;}
.cllb ul li .p1 {max-width:370px; max-height:220px; overflow:hidden;}
.cllb ul li .p1 img { display:block; transition: opacity 0.3s ease 0s;  width:100%;}
.cllb ul li:hover .p1 img { opacity:0.8;}
.cllb ul li .p2 {font-size:13px; color:#696969; line-height:20px; height:20px; overflow:hidden; margin:22px 0; transition: color 0.3s ease 0s;}
.cllb ul li .p3 { height:60px; line-height:20px; overflow:hidden; font-size:12px;}

/*翻页*/
.page_box {width:1392px; border-top:1px solid #ddd; margin:0 auto; position:relative; padding:40px 0;}
.page_box .p1 { position:absolute; left:8%; top:40px; background:url(../../static/img/zjt.gif) left 6px no-repeat; padding-left:28px; line-height:20px;}
.page_box .p2 { position:absolute; right:8%; top:40px; background:url(../../static/img/yjt.gif) right 6px no-repeat; padding-right:28px; line-height:20px;}
.page_box ul {position:relative; left:50%; float:left;}
.page_box ul li { float:left; position:relative; right:50%;  padding:0 16px; font-size:14px; line-height:20px;}
.page_box ul li a { color:#696969; }
.page_box ul .on a { color:#000; text-decoration:underline;}

/*底部*/
.footer{overflow:hidden; padding:65px 0; background:#2f3035; font-size:14px; text-align:center;}
.dblr {width:780px; margin:0 auto; color:#cbcbcc;}
.footer .div1 {padding-top:21px; line-height:22px;}
.footer .div2{padding-left:10%;padding-top:21px; line-height:22px;}
.footer .div3 { width:110px;text-align:center;padding-left:24%;font-size:12px; line-height:18px;}
.footer .div3 img{width:110px;height:110px;overflow:hidden;}





@media (min-width:1px) and (max-width:979px){
.container { width:768px; }

.yxbt {width:60%;}

.cllb {width:600px;}
.cllb ul li { float:left; width:45%; margin:0 30px 48px 0;}

.page_box {width:600px;}

.dblr {width:640px;}
.footer .div3 {padding-left:12%;}

}

@media (min-width: 980px) and (max-width:1023px){
.container { width: 980px; }


}

@media (min-width: 1024px) and (max-width:1439px) {
.container { width:1024px;}

.cllb {width:900px;}

.page_box {width:900px;}
}


@media (min-width: 1440px) and (max-width:1540px) {
.container { width:1440px; }

}

@media (min-width: 1170px) {
.container { width: 1170px; }
}


