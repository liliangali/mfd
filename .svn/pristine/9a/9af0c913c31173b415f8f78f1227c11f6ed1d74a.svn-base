{include file="header.html"}
<div id="rightTop">
    <p style="display:none;"><strong>{$lang.shipping_manage}</strong></p>
    <ul class="subnav">
    	<li><span><a href="index.php?app=shipping&act=add">添加配送方式</a></span></li>
  </ul>
</div>
<div class="tdare info">
    <table width="100%" cellspacing="0" class="dataTable">
        <!--{if $shippings}-->
        <tr class="tatr1">
            <td class="firstCell" width="15%">{$lang.shipping_name}</td>
            <!-- <td>{$lang.shipping_description}</td> -->
            <td width="5%">{$lang.enable}</td>
            <td width="10%">编码</td>
            <td width="150">{$lang.handler}</td>
        </tr>
        <!--{/if}-->
        <!--{foreach from=$shippings item=shipping}-->
        <tr class="tatr2">
            <td class="firstCell">{$shipping.shipping_name}</td>
          <!--   <td><span class="padding1">{$shipping.shipping_desc}</span></td> -->
            <td>{if $shipping.enabled}{$lang.yes}{else}{$lang.no}{/if}</td>
            <td><span class="padding1">{$shipping.code}</span></td>
            <td>
 
          <a href="index.php?app=shipping&amp;act=edit&shipping_id={$shipping.shipping_id}">编辑</a> 
          <a href="javascript:;" onclick="if(confirm('确认删除该配送方式吗？')) location.href='index.php?app=shipping&amp;act=drop&shipping_id={$shipping.shipping_id}'" class="delete">移除</a>
          {if !$shipping.is_default}<a href="index.php?app=shipping&amp;act=setdefault&amp;shipping_id={$shipping.shipping_id}">设为默认</a>{else}默认物流{/if}
             
          </td>
        </tr>
        <!--{foreachelse}-->
        <tr class="no_data">
            <td colspan="5">{$lang.no_installed_shipping}</td>
        </tr>
        <!--{/foreach}-->
    </table>
</div>

