/*--固定漂浮导航/S--*/
.fixedNav{ width:50px; position:fixed; z-index:10; top:220px; right:50%; margin-right:620px;}
.fixedNav li{ padding-top:2px; position:relative;}
.fixedNav .cur a{ background:#fb4900; color:#fff;}
.fixedNav .cur i{ position:absolute; right:-5px; bottom:0; width:0; height:0; overflow:hidden; line-height:0; border-style:solid; border-color:transparent  transparent  #fb4900 #fb4900; border-width:0 5px 5px;}
.fixedNav a{ display:block; height:50px; line-height:50px; text-align:center; background:#d7d7d7;}
@media screen and (max-width:1360px){
	.fixedNav{left:0;}
}
