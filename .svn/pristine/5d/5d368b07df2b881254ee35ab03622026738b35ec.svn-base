<!doctype html>
<html>
<head>
<meta charset="utf-8">
<meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1,minimum-scale=1, user-scalable=no, minimal-ui" >
<title>麦富迪币列表</title>
<link href="http://r.cotte.cn/cotte/wap/css/public.css" rel="stylesheet" />
<link href="http://r.cotte.cn/cotte/wap/css/slx_style.css" rel="stylesheet" />
</head>

<body>
<div class="main">
  <!--头部开始-->
    <div class="topBar">
		<div class="wrap">
			<span class="back" onClick="history.go(-1)"></span>
			<h1>我的麦富迪币</h1>
		</div>
		<div class="sta"></div>
	</div>
  <!--头部结束-->

  <ul class="ktblb">
  
  {foreach from=$list item=item}
    <li>
      <h1>{$item.name}<span class="hd">{$item.mark}{$item.cash_money}</span></h1>
      <p>{$item.add_time|date:Y/m/d  H:i:s}</p>
    </li>
  {/foreach}
  </ul>
	<div style="padding-top:20px;">
                        {include file=member.page.bottom.html}
    </div>
</div>

</body>
</html>
