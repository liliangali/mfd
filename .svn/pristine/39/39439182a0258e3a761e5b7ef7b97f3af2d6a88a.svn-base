<script src="/public/global/scrolify/jquery.scrollify.js"></script>
<script>
(function(){
	$.scrollify({
		section: '.panel',
		scrollbars: true,
	//		sectionName : "section-name",
	//		easing: "easeInBounce",
	//		scrollSpeed: 1100,
		offset : 0,
		before:function(i) {
			if(i==0){
				$('.head').slideDown('fast');
			}else{
				$('.head').slideUp('fast');		
			}
			$('#Jnav li').eq(i).addClass('cur').siblings().removeClass();
			var obj=$('.panel').eq(i);
			$('#Jnav').removeClass().addClass('pageNav nav-'+(i+1))
			obj.find('[data-animated]').each(function(i, e) {
				var cla=$(e).attr('data-animated');
				$(e).addClass(cla).one('webkitAnimationEnd mozAnimationEnd MSAnimationEnd oanimationend animationend', function(){
				  $(this).removeClass(cla);
				});
			});
		}
	});
	$('#Jnav li').click(function(){
		var i=$(this).index()+1;
		$.scrollify.instantMove("#panel-"+i);
		//$.scrollify.move("#panel-"+i);
		$('#Jnav').removeClass().addClass('pageNav nav-'+i)
	});
	$('.panel .arrow').click($.scrollify.next)
})()
</script>
