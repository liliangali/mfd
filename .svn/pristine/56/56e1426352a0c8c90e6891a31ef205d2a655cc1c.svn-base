 <div id="tab">    <h3 id="p2">配送信息</h3>    <ul class="tab_menu" id="psInfo">      <li data-id="1" class="selected">寄送地址</li>      <li data-id="2">到门店自提</li>    </ul>    <div class="tab_box">      <div class="address" id="address">        <ul>           {foreach from=$listaddress item=list name=ads}          <li {if $smarty.foreach.ads.first}class="psxxdq"{/if} data-id="{$list.addr_id}">            <p class="underlined">{$list.consignee}</p>            <p class="tele">{$list.phone_mob}</p>            <p class="txaddress">{$list.region_name}{$list.address}</p>            <div class="caozuoBtn">              <input type="button" value="编辑" class="edit" />              <input type="button" value="删除" class="del" />            </div>          </li>          {/foreach}            <li class="syxdz" id="addAddress">使用新地址</li>        </ul>      </div>      <div class="hide">        <div class="qhnear" id="qhnear">          <div class="cityselect">               <select name="city" class="selects data region_id" data-id="mendianAddress" id="selectCityAddress" title="所在城市"  validate="required">            {foreach from=$regions item=n key=k}              <option value='{$k}'>{$n}</option>                {/foreach}              </select>           </div>          <h4>请选择门面店</h4>        <div id="mendianAddress">	         {foreach from=$servers item=s name=ss}	        <dl><dd {if $smarty.foreach.ss.first}class="qhselectss"{/if} name="serverid" data-sid="{$s.idserve}">{$s.serve_name}</dd><dt>{$s.mobile}-{$s.linkman}，{$s.region_name}{$s.serve_address}</dt></dl>	         {/foreach}          </div>        </div>      </div>    </div>  </div>